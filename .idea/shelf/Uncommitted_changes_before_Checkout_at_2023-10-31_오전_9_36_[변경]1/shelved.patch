list\">\r\n        <select @change=\"sortByRating\">\r\n          <option value=\"ascending\">평점 낮은 순</option>\r\n          <option value=\"descending\">평점 높은 순</option>\r\n        </select>\r\n        <div v-for=\"(fusion, idx) in sortedItems\" :key=\"idx\">\r\n          <div class=\"list-group-item list-group-item-action\" @click=\"showModal(fusion)\">\r\n            <ModalView v-if=\"fusion.modalVisible\">\r\n              <template v-slot:modalText>\r\n                <div class=\"card shadow-sm\">\r\n                  <div class=\"card-body\">\r\n                    <div class=\"mtext-and-image-container\">\r\n                      <div class=\"mdetails\">\r\n                        <div class=\"mtext-content\">\r\n                          <span><h3 class=\"shopTitle\"> {{fusion.storename}} </h3></span>\r\n                          <span class=\"shop1\"> {{fusion.category}} </span> &nbsp;\r\n                          <span><i class=\"fa fa-star\" aria-hidden=\"true\" style=\"color: green\"></i>&nbsp;{{fusion.rating}} </span><br>\r\n                          <span><i class=\"fa fa-phone\" aria-hidden=\"true\"></i>&nbsp;{{fusion.phonenumber}}</span><br>\r\n                          <span><i class=\"fa fa-clock-o\" aria-hidden=\"true\"></i>&nbsp;{{fusion.detailinfo}}</span><br>\r\n                          <span class=\"shop2\"><i class=\"fa fa-map-marker\" aria-hidden=\"true\"></i>&nbsp; {{fusion.location}} </span>\r\n                        </div>\r\n                        <div class=\"mimage-content\">\r\n                          <img :src=\"setImgUrl(fusion.image)\" class=\"mimglist\" style=\"width: 300px; height: 220px;\"/>\r\n                        </div>\r\n                      </div>\r\n\r\n                      <div class=\"anotherimage\">\r\n                        <div><img :src=\"setImgUrl(fusion.image1)\" class=\"ming\" /></div>\r\n                        <div><img :src=\"setImgUrl(fusion.image2)\" class=\"ming\" /></div>\r\n                        <div><img :src=\"setImgUrl(fusion.image3)\" class=\"ming\" /></div>\r\n                        <div><img :src=\"setImgUrl(fusion.image4)\" class=\"ming\" /></div>\r\n                        <div><img :src=\"setImgUrl(fusion.image5)\" class=\"ming\" /></div>\r\n                      </div>\r\n\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </template>\r\n            </ModalView>\r\n            <div class=\"card-body\">\r\n              <div class=\"text-and-image-container\">\r\n                <div class=\"text-content\">\r\n                  <span><h3 class=\"shopTitle\"> {{fusion.storename}} </h3></span>\r\n                  <span class=\"shop1\"> {{fusion.category}} </span> &nbsp;\r\n                  <span><i class=\"fa fa-star\" aria-hidden=\"true\" style=\"color: green\"></i>{{fusion.rating}} </span><br>\r\n                  <span class=\"shop2\"> <i class=\"fa fa-map-marker\" aria-hidden=\"true\"></i>&nbsp;{{fusion.location}} </span>\r\n                </div>\r\n                <div class=\"image-content\">\r\n                  <img :src=\"setImgUrl(fusion.image)\" class=\"imglist\" />\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div> <!-- v-for-->\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, onMounted } from \"vue\";\r\nimport axios from \"axios\";\r\nimport Header from \"@/components/header/Header.vue\";\r\nimport ModalView from \"@/components/foodCategory/ModalView.vue\";\r\n\r\nexport default {\r\n  setup() {\r\n    const state = reactive({\r\n      items: [],\r\n    });\r\n\r\n    onMounted(() => {\r\n      axios.get(\"/api/shop/fusion\").then((response) => {\r\n        state.items = response.data;\r\n      });\r\n    });\r\n\r\n    const showModal = (fusion) => {\r\n      fusion.modalVisible = !fusion.modalVisible;\r\n      console.log(fusion.storename)\r\n      console.log(fusion.loca