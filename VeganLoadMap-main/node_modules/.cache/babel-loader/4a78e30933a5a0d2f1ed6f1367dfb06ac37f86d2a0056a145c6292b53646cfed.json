iew.star\" class=\"star-radio\">\n        <label for=\"star1\">\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n        </label>\n\n        <input type=\"radio\" id=\"star2\" value=\"2\" v-model=\"review.star\" class=\"star-radio\">\n        <label for=\"star2\">\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n        </label>\n\n        <input type=\"radio\" id=\"star3\" value=\"3\" v-model=\"review.star\" class=\"star-radio\">\n        <label for=\"star3\">\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n        </label>\n\n        <input type=\"radio\" id=\"star4\" value=\"4\" v-model=\"review.star\" class=\"star-radio\">\n        <label for=\"star4\">\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n        </label>\n\n        <input type=\"radio\" id=\"star5\" value=\"5\" v-model=\"review.star\" class=\"star-radio\">\n        <label for=\"star5\">\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n          <i class=\"fa fa-star\" aria-hidden=\"true\"></i>\n        </label>\n      </div>\n      <input ref=\"image\" @change=\"uploadImg()\" type=\"file\" id=\"chooseFile\" name=\"chooseFile\" accept=\"image/*\">\n      <button type=\"submit\">등록</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport router from \"@/scripts/router\";\n\nexport default {\n  data() {\n    return {\n      review: {\n        content: '',\n        image: '',\n        star:'',\n      },\n    };\n  },\n  methods: {\n    reviewInsert() {\n      const storename = this.$route.params.value;\n      axios\n          .put(`/api/review/insert/${storename}`, this.review)\n          .then(() => {\n            window.alert(\"리뷰정보가  입력되었습니다\");\n            router.push({path: \"/myreview\"});\n          })\n          .catch((error) => {\n            console.error(\"리뷰 업데이트 중 오류 발생\", error);\n          });\n    },\n    uploadImg() {\n      var image = this.$refs[\"image\"].files[0];\n      const url = URL.createObjectURL(image);\n      this.image = url;\n      this.review.image = this.image;\n      console.log(url);\n      console.log(this.image);\n    },\n  },\n};\n</script>\n\n<style scoped>\n* {\n  box-sizing: border-box;\n}\n\nheader {\n  width: 80%;\n  font-size: 2em;\n  text-align: center;\n  background: #2ecc71;\n  color: #fff;\n  margin: 0 auto;\n  padding: 0.5em;\n}\ninput[type=\"radio\"]:checked + label i {\n  color: #FEC902; /* 원하는 색상으로 변경하세요 */\n}\n.star-radio {\n  /* radio 버튼을 숨기기 위한 방법 */\n  position: absolute;\n  left: -9999px;\n}\nbody {\n  max-width: 1024px;\n  margin: 20px auto;\n  font-size: 16px;\n  font-family: 'Poppins', sans-serif;\n}\n\nimg {\n  max-width: 100%;\n  display: block;\n}\n\np {\n  margin: 0;\n}\n\nlabel {\n  width: 15%;\n  margin: 10px 1% 0 0;\n  display: inline-block;\n  float: left;\n}\n\ninput[type=\"text\"] {\n  width: 84%;\n  margin: 10px 0;\n  height: 40px;\n  border: none;\n  background: #F6F6F6;\n  padding: 1em;\n}\n\ntextarea {\n  width: 84%;\n  border: none;\n  background: #F6F6F6;\n  margin: 10px auto;\n  height: 100px;\n  padding: 1em;\n  border-radius: 20px;\n  text-align: center;\n\n}\n\nbutton {\n  float: right;\n  width: 20%;\n  height: 40px;\n  border: none;\n  background: #2ecc71;\n  font-size: 16px;\n  color: white;\n}\n\n.review {\n  width: 100%;\n  display: inline-block;\n  margin: 10px 0;\n}\n\n.review-photo {\n  height: 80px;\n  width: 80px;\n  display: inline-block;\n  float: left;\n  margin-right: 3em;\n}\n\n.review-box {\n  float: left;\n  display: inline-block;\n  width: 87.5%;\n  background: #F6F6F6;\n  padding: 1.5em;\n  position: relative;\n}\n\n.review-box:after, .review-box:before {\n  right: 100%;\n  top: 10%;\n  border: solid transparent;\n  content: \" \";\n  height: 0;\n  width: 0;\n  position: absolute;\n  pointer-events: none;\n}\n\n.review-box:after {\n  border-right-color: #F6F6F6;\n  border-width: 25px;\n}\n\n.review-comment {\n  color: #9A9999;\n}\n\n.review-date {\n  color: #C7C7C7;\n}\n\n.review-author {\n  color: #C7C7C7;\n}\n\n.review-author strong {\n  color: black;\n}\n</style>"],"mappings":";;;;AAmDA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,kBAAkB;AAErC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QACNC,OAAO,EAAE,EAAE;QACXC,KAAK,EAAE,EAAE;QACTC,IAAI,EAAC;MACP;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,MAAMC,SAAQ,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,KAAK;MAC1CZ,KAAI,CACCa,GAAG,CAAE,sBAAqBJ,SAAU,EAAC,EAAE,IAAI,CAACN,MAAM,EAClDW,IAAI,CAAC,MAAM;QACVC,MAAM,CAACC,KAAK,CAAC,gBAAgB,CAAC;QAC9Bf,MAAM,CAACgB,IAAI,CAAC;UAACC,IAAI,EAAE;QAAW,CAAC,CAAC;MAClC,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACD,KAAK,CAAC,iBAAiB,EAAEA,KAAK,CAAC;MACzC,CAAC,CAAC;IACR,CAAC;IACDE,SAASA,CAAA,EAAG;MACV,IAAIjB,KAAI,GAAI,IAAI,CAACkB,KAAK,CAAC,OAAO,CAAC,CAACC,KAAK,CAAC,CAAC,CAAC;MACxC,MAAMC,GAAE,GAAIC,GAAG,CAACC,eAAe,CAACtB,KAAK,CAAC;MACtC,IAAI,CAACA,KAAI,GAAIoB,GAAG;MAChB,IAAI,CAACtB,MAAM,CAACE,KAAI,GAAI,IAAI,CAACA,KAAK;MAC9BgB,OAAO,CAACO,GAAG,CAACH,GAAG,CAAC;MAChBJ,OAAO,CAACO,GAAG,CAAC,IAAI,CAACvB,KAAK,CAAC;IACzB;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}