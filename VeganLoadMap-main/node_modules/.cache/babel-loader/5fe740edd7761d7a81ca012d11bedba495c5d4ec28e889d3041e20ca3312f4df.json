{"ast":null,"code":"import axios from \"axios\";\nimport { reactive } from \"vue\";\nimport lib from \"@/scripts/lib\";\nexport default {\n  name: \"Store\",\n  setup() {\n    const state = reactive({\n      menus: []\n    });\n    axios.get(\"/api/menu\").then(({\n      data\n    }) => {\n      state.menus = data;\n    });\n    const addToCart = menuId => {\n      axios.post(`/api/cart/menu/${menuId}`).then(() => {\n        console.log('success');\n      });\n    };\n    return {\n      state,\n      lib,\n      addToCart\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","lib","name","setup","state","menus","get","then","data","addToCart","menuId","post","console","log"],"sources":["/Users/soohykim/Desktop/veganloadmap/src/pages/Store.vue"],"sourcesContent":["<template>\n  <div class=\"store\">\n    <div class=\"album py-5 bg-body-tertiary\">\n      <div class=\"container\">\n\n        <div class=\"row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3\">\n          <div class=\"col\" v-for=\"(menu, idx) in state.menus\" :key=\"idx\">\n            <div class=\"card shadow-sm\">\n              <!--      <span class=\"img\" style=\"{backgroundImage: url('${item.imagePath}')}\" />-->\n              <img :src=\"menu.image\"/>\n              <div class=\"card-body\">\n                <p class=\"card-text\">\n                  <span> {{menu.menu}} </span> &nbsp;\n                  <span class=\"discount badge bg-danger\">{{menu.sale}}%</span>\n                </p>\n                <div class=\"d-flex justify-content-between align-items-center\">\n                  <small class=\"price text-muted\">\n                    {{lib.getNumberFormatted(menu.price)}}원\n                  </small>\n                  <small class=\"real text-danger\">\n                    {{lib.getNumberFormatted(menu.price - (menu.price * menu.sale / 100))}}원\n                  </small>\n                  <button class=\"btn btn-primary\" @click=\"addToCart(menu.seq)\">\n<!--                  <button class=\"btn btn-primary\" @click=\"$router.push('/reserve')\">-->\n                    <i class=\"fa fa-calendar-check-o\" aria-hidden=\"true\"></i>\n                  </button>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n  </div>\n\n</template>\n\n<script>\nimport axios from \"axios\";\nimport {reactive} from \"vue\";\nimport lib from \"@/scripts/lib\";\n\nexport default {\n  name: \"Store\",\n  setup() {\n    const state = reactive({\n      menus: []\n    })\n    axios.get(\"/api/menu\").then(({data}) => {\n      state.menus = data;\n    })\n\n    const addToCart = (menuId) => {\n      axios.post(`/api/cart/menu/${menuId}`).then(() => {\n        console.log('success')\n      })\n    };\n\n    return {state, lib, addToCart};\n  }\n}\n</script>\n\n<style scoped>\n.card .img{\n  display: inline-block;\n  width: 100%;\n  hieght: 250px;\n  background-size: cover;\n  background-position: center;\n}\n\n.btn {\n  width:80px;\n  float: right;\n}\n\n.card .card-body .price{\n  text-decoration: line-through;\n}\n</style>"],"mappings":"AAuCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAAQC,QAAQ,QAAO,KAAK;AAC5B,OAAOC,GAAE,MAAO,eAAe;AAE/B,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIJ,QAAQ,CAAC;MACrBK,KAAK,EAAE;IACT,CAAC;IACDN,KAAK,CAACO,GAAG,CAAC,WAAW,CAAC,CAACC,IAAI,CAAC,CAAC;MAACC;IAAI,CAAC,KAAK;MACtCJ,KAAK,CAACC,KAAI,GAAIG,IAAI;IACpB,CAAC;IAED,MAAMC,SAAQ,GAAKC,MAAM,IAAK;MAC5BX,KAAK,CAACY,IAAI,CAAE,kBAAiBD,MAAO,EAAC,CAAC,CAACH,IAAI,CAAC,MAAM;QAChDK,OAAO,CAACC,GAAG,CAAC,SAAS;MACvB,CAAC;IACH,CAAC;IAED,OAAO;MAACT,KAAK;MAAEH,GAAG;MAAEQ;IAAS,CAAC;EAChC;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}