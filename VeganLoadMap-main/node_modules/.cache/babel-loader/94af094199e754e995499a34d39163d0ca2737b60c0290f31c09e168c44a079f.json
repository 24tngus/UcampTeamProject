g","$setup","state","form","name","_hoisted_11","_hoisted_12","_hoisted_13","_hoisted_14","type","$event","_hoisted_15","_hoisted_16","password","_hoisted_17","_hoisted_18","_hoisted_19","_hoisted_20","email","_hoisted_21","_hoisted_22","tel","_hoisted_23","_hoisted_24","_hoisted_25","storenumber","_hoisted_26","_hoisted_27","_hoisted_28","storefile","_hoisted_29","_hoisted_30","onClick","_cache","deleteStore","from","seq","mystoreUpdate","_hoisted_31"],"sources":["/Users/soohykim/Desktop/front/src/components/store/MystoreUpdate.vue"],"sourcesContent":["<template>\n  <Header2 />\n  <div id=\"wrapper\">\n\n    <div id=\"container\">\n      <div class=\"tab\">\n        <router-link to=\"/mystore\"><h1>마이페이지</h1></router-link>\n        <div class=\"buttontab\">\n          <router-link to=\"/mystore_info\"><button class=\"tablink\">회원 정보</button></router-link>\n          <router-link to=\"/myshop_info\"><button class=\"tablink\">가게 정보</button></router-link>\n          <router-link to=\"/reserve_select\"><button class=\"tablink\">예약 확인</button></router-link>\n          <router-link to=\"/review_store\"><button class=\"tablink\">리뷰 확인</button></router-link>\n        </div>\n      </div>\n\n      <!-- 본문 작성 -->\n      <div class=\"online small\" id=\"online\">\n        <h1>{{state.form.name}}님 회원 정보 수정</h1>\n        <div class=\"member\">\n          <div class=\"field\">\n            <b>아이디</b>\n            <span class=\"placehold-text\">\n              <input type=\"text\" class=\"block\" v-model=\"state.form.id\">\n      </span>\n          </div>\n          <div class=\"field\">\n            <b>비밀번호</b>\n            <input type=\"password\" class=\"block\" v-model=\"state.form.password\">\n          </div>\n          <div class=\"field\">\n            <b>이름</b>\n            <input type=\"text\" class=\"block\" v-model=\"state.form.name\">\n          </div>\n          <div class=\"field\">\n            <b>이메일</b>\n            <input type=\"email\" class=\"block\" v-model=\"state.form.email\">\n          </div>\n          <div class=\"field tel-number\">\n            <b>휴대전화</b>\n            <div>\n              <input type=\"text\" class=\"block\" v-model=\"state.form.tel\">\n            </div>\n          </div>\n          <div class=\"field\">\n            <b>사업자 등록 번호</b>\n            <div class=\"block\">{{state.form.storenumber}}</div>\n          </div>\n          <div class=\"field\">\n            <b>사업자 등록증</b>\n            <div class=\"block\">{{state.form.storefile}}</div>\n          </div><br>\n          <div class=\"button-container\">\n            <button class=\"btn\" @click=\"deleteStore(state.from.seq)\">삭제</button>\n            <button class=\"btn\" @click=\"mystoreUpdate()\">확인</button>\n          </div>\n        </div>\n        <!-- 페이지 처리 -->\n        <div id=\"num\">\n          <span><a href=\"#\"> &lt; </a></span>\n          <span><a href=\"\">1</a></span>\n          <span><a href=\"#\"> > </a></span>\n        </div>\n        <br><br><br><br><br><br><br><br><br><br>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n\nimport {reactive} from \"vue\";\nimport axios from \"axios\";\nimport router from \"@/scripts/router\";\nimport Header2 from \"@/components/header/Header2.vue\";\n\nexport default {\n  name: \"MystoreUpdate\",\n  components: {Header2},\n  setup() {\n    const state = reactive({\n      form :{\n        seq: \"\",\n        id: \"\",\n        password: \"\",\n        name: \"\",\n        email: \"\",\n        tel: \"\",\n        storenumber: \"\",\n        storefile: \"\"\n      }\n    })\n    const load = () => {\n      axios.get(\"/api/store/mypage\").then(({data}) => {\n        state.form.seq = data.seq;\n        state.form.id=data.id;\n        state.form.password=data.password;\n        state.form.name=data.name;\n        state.form.email=data.email;\n        state.form.tel=data.tel;\n        state.form.storenumber=data.storenumber;\n        state.form.storefile=data.storefile;\n      })\n    };\n\n    load();\n    const mystoreUpdate = () => {\n      const updateData = {\n        id: state.form.id,\n        password: state.form.password,\n        name: state.form.name,\n        email: state.form.email,\n        tel: state.form.tel,\n        storenumber: state.form.storenumber,\n        storefile: state.form.storefile\n      }\n      axios.put(\"/api/store/mypage/update\", updateData).then(()=> {\n        window.alert(\"회원 정보가 수정 되었습니다\");\n        router.push({path: \"/mystore_info\"});\n      })\n    }\n\n    const deleteStore = (seq) => {\n      axios.delete(`/api/shop/register/delete/${seq}`).then(() => {\n        window.alert(\"가게 정보가 삭제 되었습니다\");\n        router.push({path: \"/mystore\"});\n      })\n    }\n\n    return {state, mystoreUpdate, deleteStore}\n  }\n}\n\n</script>\n\n<style scoped>\n@import url('https://fonts.googleapis.com/css?family=Nanum+Gothic:700,800&subset=korean');\n\n*,html,body{\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  color: #333;\n  font-size: 15px;\n  font-family: 'Nanum Gothic', sans-serif, '굴림', 'gulim'\n}\n\nimg{border: none}\n\na{color: #333;\n  text-decoration:none;\n}\n\nul li{\n  list-style: none;\n}\n\n#wrapper{\n  width: 100%;\n}\n\n.status{\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 21px;\n}\n.item{\n  display: flex;\n}\n.number{\n  font-size: 25px;\n  font-weight: 500;\n  text-align: center;\n}\n.item_text{\n  font-size: 11px;\n  font-weight: normal;\n  color: #c2c2c2;\n  text-align: center;\n}\n.item_icon{\n  display: flex;\n  align-items: center;\n  padding: 20px;\n  width: 16px;\n  height: 16px;\n}\n\n#num{\n  text-align: center;\n  margin: 30px 0;\n  padding: 40px 0;\n  position: relative;\n  z-index: 2\n}\n#num:before{\n  content: '';\n  position: absolute;\n  width: 100%;\n  height: 100%;\n  top:0px;\n  left: 0;\n  border-top: 1px solid #d4d4d4;\n  z-index: -1\n}\n#num span{\n  display: inline-block;\n  border: 1px solid #d4d4d4;\n  margin: 0 2px;\n  padding: 5px 10px;\n  vertical-align: middle;\n  cursor: pointer\n}\n#num span a{\n  font-size: 10px;\n}\n.currentNum{\n  background: #32312f!important;\n}\n.currentNum a{\n  color: #fff\n}\n#num span:hover{\n  background: #d4d4d4;\n}\n#num span:hover a,#num span:hover i{\n  color: #fff\n}\n\n#notice:after{\n  content: '';\n  display: block;\n  clear: both;\n}\n#notice ul{\n  display: inline-block;\n  height:25px;\n  line-height: 25px;\n  width: 70%;\n  text-align: left;\n  padding-left: 15%;\n  float: left;\n  overflow: hidden;\n  position: relative;\n  background-size: 20px;\n}\n#notice ul:after{\n  content: '';\n  position: absolute;\n  width: 20px;\n  height: 25px;\n  line-height: 25px;\n  background-size: 20px;\n  z-index: 2000;\n  top:4px;\n  left: 16%;\n  z-index: 120;\n}\n\n#noticewrap a, #noticewrap i{\n  color: #fff;\n}\n#noticewrap ul a{\n  font-size: 0.7em;\n}\n\n#noticewrap p{\n  display: inline-block;\n  line-height: 25px;\n}\n\n#wrapper{\n  background: #f1f1f1\n}\n#wrapper header {\n  position: relative !important;\n  background: #fff!important\n}\n#wrapper .headerwrap a, #wrapper .user i{\n  color: #333\n}\n#container{\n  position: relative;\n  max-width: 70%;\n  min-width: 60%;\n  margin: 0 0 0 20%;\n  background: #fff;\n}\n\n#container:after{\n  content:'';\n  display: block;\n  clear: both\n}\n#container > div:not(:first-of-type){\n  background: #fff;\n  padding: 40px 20px 0 250px;\n\n}\n.tab{\n  position: absolute;\n  left: 0;\n  width: 200px;\n  height: 100%;\n  /*    float: left;*/\n  /*    background: pink;*/\n  height: 100%;\n  /*    position: relative;*/\n  padding: 200px 20px 0 20px;\n}\n.tab:after{\n  content: '';\n  display: block;\n  clear:both\n}\nbutton{\n  border: none;\n  background: none;\n  outline: none;\n  cursor: pointer;\n  width: 100px;\n  height: 70px;\n}\nbutton.on{\n  color: #fa2828;\n  font-weight: 900\n}\n.tab h1{\n  position: absolute;\n  top: 0;\n  left: 0;\n  /*    float: left;*/\n  height: 150px;\n  width: 100%;\n  backgrou