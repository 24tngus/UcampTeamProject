{"ast":null,"code":"import axios from \"axios\";\nimport { reactive } from \"vue\";\nimport Header from \"@/components/header/Header.vue\";\nexport default {\n  name: \"Shop\",\n  components: {\n    Header\n  },\n  setup() {\n    const state = reactive({\n      items: []\n    });\n    const setImgUrl = imageName => {\n      return `https://maps.googleapis.com/maps/api/place/photo?maxwidth=400&photo_reference=${imageName}&key=AIzaSyB21eMRg-uMRk-i7r27DPDkntXbR5_kvnk`;\n    };\n    axios.get(\"/api/shop\").then(({\n      data\n    }) => {\n      state.items = data;\n    });\n    const addToCart = shopSeq => {\n      axios.post(`/api/cart/shop/${shopSeq}`).then(() => {\n        console.log('success');\n      });\n    };\n    return {\n      state,\n      setImgUrl,\n      addToCart\n    };\n  },\n  data() {\n    return {\n      imgUrl: null\n    };\n  },\n  methods: {\n    // convertImg(url) {\n    //   const firstUrl = `https://maps.googleapis.com/maps/api/place/photo?maxwidth=400&photo_reference=${url}&key=AIzaSyB21eMRg-uMRk-i7r27DPDkntXbR5_kvnk`\n\n    // }\n  }\n};","map":{"version":3,"names":["axios","reactive","Header","name","components","setup","state","items","setImgUrl","imageName","get","then","data","addToCart","shopSeq","post","console","log","imgUrl","methods"],"sources":["/Users/soohykim/Desktop/front/src/pages/Shop.vue"],"sourcesContent":["<template>\n  <Header/>\n  <div class=\"shop\">\n    <div class=\"album py-5 bg-body-tertiary\">\n      <div class=\"container\">\n\n        <div class=\"row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3\">\n          <div class=\"col\" v-for=\"(shop, idx) in state.items\" :key=\"idx\">\n            <div class=\"card shadow-sm\">\n              <img :src=\"shop.image\" v-if=\"shop.image !== null\" />\n              <img :src=\"setImgUrl(shop.image1)\" v-else />\n              <div class=\"card-body\">\n                <p class=\"card-text\">\n                  <span> {{shop.storename}} </span> &nbsp;\n                </p>\n                <div class=\"d-flex justify-content-between align-items-center\">\n                  <button class=\"btn btn-primary\" @click=\"$router.push({name:'menu', params: {value: shop.seq}})\">\n                    <i class=\"fa fa-plus\" aria-hidden=\"true\"></i>\n                  </button>\n                  <p>{{shop.seq}}</p>\n                  <!--                  <button class=\"btn btn-primary\" @click=\"addToCart(shop.seq)\">-->\n                  <!--                    <i class=\"fa fa-shopping-cart\" aria-hidden=\"true\"></i>-->\n                  <!--                  </button>-->\n                  <button class=\"btn btn-primary\" @click=\"$router.push('/reserve')\">\n                    <i class=\"fa fa-calendar-check-o\" aria-hidden=\"true\"></i>\n                  </button>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n  </div>\n\n</template>\n\n<script>\nimport axios from \"axios\";\nimport {reactive} from \"vue\";\nimport Header from \"@/components/header/Header.vue\";\n\nexport default {\n  name: \"Shop\",\n  components: {Header},\n  setup() {\n    const state = reactive({\n      items: []\n    })\n\n    const setImgUrl = (imageName) => {\n      return `https://maps.googleapis.com/maps/api/place/photo?maxwidth=400&photo_reference=${imageName}&key=AIzaSyB21eMRg-uMRk-i7r27DPDkntXbR5_kvnk`;\n    };\n\n    axios.get(\"/api/shop\").then(({data}) => {\n      state.items = data;\n    })\n\n    const addToCart = (shopSeq) => {\n      axios.post(`/api/cart/shop/${shopSeq}`).then(() => {\n        console.log('success')\n      })\n    };\n\n    return {state, setImgUrl, addToCart};\n  },\n  data() {\n    return {\n      imgUrl: null,\n    }\n  },\n  methods: {\n    // convertImg(url) {\n    //   const firstUrl = `https://maps.googleapis.com/maps/api/place/photo?maxwidth=400&photo_reference=${url}&key=AIzaSyB21eMRg-uMRk-i7r27DPDkntXbR5_kvnk`\n\n    // }\n  }\n}\n</script>\n\n<style scoped>\n.card .img{\n  display: inline-block;\n  width: 100%;\n  hieght: 250px;\n  background-size: cover;\n  background-position: center;\n}\n\n.btn {\n  width:80px;\n  float: right;\n}\n\n.card .card-body .price{\n  text-decoration: line-through;\n}\n</style>"],"mappings":"AAuCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAAQC,QAAQ,QAAO,KAAK;AAC5B,OAAOC,MAAK,MAAO,gCAAgC;AAEnD,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IAACF;EAAM,CAAC;EACpBG,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,CAAC;MACrBM,KAAK,EAAE;IACT,CAAC;IAED,MAAMC,SAAQ,GAAKC,SAAS,IAAK;MAC/B,OAAQ,iFAAgFA,SAAU,8CAA6C;IACjJ,CAAC;IAEDT,KAAK,CAACU,GAAG,CAAC,WAAW,CAAC,CAACC,IAAI,CAAC,CAAC;MAACC;IAAI,CAAC,KAAK;MACtCN,KAAK,CAACC,KAAI,GAAIK,IAAI;IACpB,CAAC;IAED,MAAMC,SAAQ,GAAKC,OAAO,IAAK;MAC7Bd,KAAK,CAACe,IAAI,CAAE,kBAAiBD,OAAQ,EAAC,CAAC,CAACH,IAAI,CAAC,MAAM;QACjDK,OAAO,CAACC,GAAG,CAAC,SAAS;MACvB,CAAC;IACH,CAAC;IAED,OAAO;MAACX,KAAK;MAAEE,SAAS;MAAEK;IAAS,CAAC;EACtC,CAAC;EACDD,IAAIA,CAAA,EAAG;IACL,OAAO;MACLM,MAAM,EAAE;IACV;EACF,CAAC;EACDC,OAAO,EAAE;IACP;IACA;;IAEA;EAAA;AAEJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}