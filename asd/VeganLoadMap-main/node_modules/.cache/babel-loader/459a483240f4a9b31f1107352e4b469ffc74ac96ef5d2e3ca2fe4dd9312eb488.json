{"ast":null,"code":"import { reactive } from \"vue\";\nimport axios from \"axios\";\nimport lib from \"@/scripts/lib\";\nimport Header from \"@/components/header/Header.vue\";\nexport default {\n  components: {\n    Header\n  },\n  setup() {\n    const state = reactive({\n      items: []\n    });\n    const load = () => {\n      axios.get(\"/api/cart/shop\").then(({\n        data\n      }) => {\n        console.log(\"load\");\n        state.items = data;\n      });\n    };\n    const remove = shopSeq => {\n      axios.delete(`/api/cart/shop/${shopSeq}`).then(() => {\n        console.log(\"delete\", this.shopseq);\n        load();\n      });\n    };\n    load();\n    return {\n      state,\n      lib,\n      remove\n    };\n  }\n};","map":{"version":3,"names":["reactive","axios","lib","Header","components","setup","state","items","load","get","then","data","console","log","remove","shopSeq","delete","shopseq"],"sources":["/Users/soohykim/Desktop/front/src/components/user/Cart.vue"],"sourcesContent":["<template>\n  <Header/>\n  <div class=\"cart\">\n    <div class=\"container\">\n      <ul>\n        <li v-for=\"(shop, idx) in state.items\" :key=\"idx\">\n          <img :src=\"shop.image\"/>\n          <span class=\"name\">{{ shop.storename }}</span>\n          <span class=\"category\">{{shop.category}}</span>\n          <span class=\"category\">예약금{{ lib.getNumberFormatted(10000) }}원</span>\n          <i class=\"fa fa-trash\" @click=\"remove(shop.seq)\"></i>\n        </li>\n      </ul>\n      <router-link to=\"/reserve\" class=\"btn btn-primary\">예약하기</router-link>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {reactive} from \"vue\";\nimport axios from \"axios\";\nimport lib from \"@/scripts/lib\";\nimport Header from \"@/components/header/Header.vue\";\n\nexport default {\n  components: {Header},\n  setup() {\n    const state = reactive({\n      items: []\n    })\n\n    const load = () => {\n      axios.get(\"/api/cart/shop\").then(({data}) => {\n        console.log(\"load\");\n        state.items = data;\n      })\n    };\n\n    const remove = (shopSeq) => {\n      axios.delete(`/api/cart/shop/${shopSeq}`).then(() => {\n        console.log(\"delete\", this.shopseq);\n        load();\n      })\n    }\n\n    load();\n\n    return {state, lib, remove}\n  }\n}\n</script>\n\n<style scoped>\n.cart ul {\n  list-style: none;\n  margin: 0;\n  padding: 0;\n}\n\n.cart ul li {\n  border: 1px solid #eee;\n  margin-top: 25px;\n  margin-bottom: 25px;\n}\n\n.cart ul li img {\n  width: 150px;\n  height: 150px;\n}\n\n.cart ul li .name {\n  margin-left: 25px;\n}\n\n.cart ul li .category {\n  margin-left: 25px;\n}\n\n.cart ul li i {\n  float: right;\n  font-size: 20px;\n  margin-top: 65px;\n  margin-right: 50px;\n}\n\n.cart .btn {\n  width:300px;\n  display:block;\n  margin:0 auto;\n  padding:30px 50px;\n  font-size: 20px;\n}\n</style>"],"mappings":"AAmBA,SAAQA,QAAQ,QAAO,KAAK;AAC5B,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,GAAE,MAAO,eAAe;AAC/B,OAAOC,MAAK,MAAO,gCAAgC;AAEnD,eAAe;EACbC,UAAU,EAAE;IAACD;EAAM,CAAC;EACpBE,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC;MACrBO,KAAK,EAAE;IACT,CAAC;IAED,MAAMC,IAAG,GAAIA,CAAA,KAAM;MACjBP,KAAK,CAACQ,GAAG,CAAC,gBAAgB,CAAC,CAACC,IAAI,CAAC,CAAC;QAACC;MAAI,CAAC,KAAK;QAC3CC,OAAO,CAACC,GAAG,CAAC,MAAM,CAAC;QACnBP,KAAK,CAACC,KAAI,GAAII,IAAI;MACpB,CAAC;IACH,CAAC;IAED,MAAMG,MAAK,GAAKC,OAAO,IAAK;MAC1Bd,KAAK,CAACe,MAAM,CAAE,kBAAiBD,OAAQ,EAAC,CAAC,CAACL,IAAI,CAAC,MAAM;QACnDE,OAAO,CAACC,GAAG,CAAC,QAAQ,EAAE,IAAI,CAACI,OAAO,CAAC;QACnCT,IAAI,CAAC,CAAC;MACR,CAAC;IACH;IAEAA,IAAI,CAAC,CAAC;IAEN,OAAO;MAACF,KAAK;MAAEJ,GAAG;MAAEY;IAAM;EAC5B;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}