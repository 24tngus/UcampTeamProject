{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { reactive } from \"vue\";\nimport axios from \"axios\";\nimport store from \"@/scripts/store\";\nimport router from \"@/scripts/router\";\nimport Header from \"@/components/header/Header.vue\";\nexport default {\n  components: {\n    Header\n  },\n  setup() {\n    const state = reactive({\n      form: {\n        email: \"\",\n        password: \"\"\n      }\n    });\n    const submit = () => {\n      axios.post(\"/api/store/login\", state.form).then(res => {\n        store.commit(\"setAccount\", res.data); // store 저장\n        if (res.data == 0) {\n          if (state.form.email == \"\") {\n            window.alert(\"이메일을 입력해주세요\");\n          } else if (state.form.password == \"\") {\n            window.alert(\"비밀번호를 입력해주세요\");\n          } else {\n            window.alert(\"이메일이나 비밀번호가 틀립니다\");\n            router.push({\n              path: \"/login2\"\n            });\n          }\n        } else {\n          // console.log(res.data);\n          window.alert(\"로그인 하였습니다\");\n          router.push({\n            path: \"/home_store\"\n          });\n        }\n      });\n    };\n    return {\n      state,\n      submit\n    };\n  }\n};","map":{"version":3,"names":["reactive","axios","store","router","Header","components","setup","state","form","email","password","submit","post","then","res","commit","data","window","alert","push","path"],"sources":["/Users/soohykim/Desktop/front/src/components/store/Login2.vue"],"sourcesContent":["<template>\n  <Header />\n  <br><br>\n  <div class=\"container\" id=\"container\">\n    <div class=\"form-container sign-in-container\">\n      <form action=\"#\">\n        <h1>Sign In</h1>\n        <div class=\"social-container\">\n          <a href=\"#\" class=\"social\"><i class=\"fa fa-facebook-f\" aria-hidden=\"true\"></i></a>\n          <a href=\"#\" class=\"social\"><i class=\"fa fa-google-plus\" aria-hidden=\"true\"></i></a>\n          <a href=\"#\" class=\"social\"><i class=\"fa fa-comments\" aria-hidden=\"true\"></i></a>\n        </div>\n        <span>or use your account</span>\n        <input type=\"email\" placeholder=\"Email\" v-model=\"state.form.email\">\n        <input type=\"password\" placeholder=\"Password\" v-model=\"state.form.password\" /><br>\n        <button @click=\"submit()\">로그인</button>\n      </form>\n    </div>\n    <div class=\"overlay-container\">\n      <div class=\"overlay\">\n        <div class=\"overlay-panel overlay-right\">\n          <h1>VeganRoadMap</h1>\n          <p>비건로드맵에 가게를 등록하고 싶으신가요?</p>\n          <button class=\"ghost\" id=\"signUp\" @click=\"$router.push('/join2')\">회원가입</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {reactive} from \"vue\";\nimport axios from \"axios\";\nimport store from \"@/scripts/store\";\nimport router from \"@/scripts/router\";\nimport Header from \"@/components/header/Header.vue\";\n\nexport default {\n  components: {Header},\n  setup() {\n    const state = reactive({\n      form :{\n        email: \"\",\n        password: \"\"\n      }\n    })\n    const submit = () => {\n      axios.post(\"/api/store/login\", state.form).then((res)=> {\n        store.commit(\"setAccount\", res.data); // store 저장\n        if (res.data == 0) {\n          if (state.form.email == \"\") {\n            window.alert(\"이메일을 입력해주세요\");\n          } else if (state.form.password == \"\") {\n            window.alert(\"비밀번호를 입력해주세요\");\n          } else {\n            window.alert(\"이메일이나 비밀번호가 틀립니다\");\n            router.push({path: \"/login2\"});\n          }\n        }else {\n          // console.log(res.data);\n          window.alert(\"로그인 하였습니다\");\n          router.push({path: \"/home_store\"});\n        }\n      })\n    }\n    return {state, submit}\n  }\n}\n</script>\n\n<style scoped>\n@import url('https://fonts.googleapis.com/css?family=Montserrat:400,800');\n\n* {\n  box-sizing: border-box;\n}\n\nbody {\n  background: #f6