{"ast":null,"code":"import { reactive, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport Header from \"@/components/header/Header.vue\";\nimport ModalView from \"@/components/foodCategory/ModalView.vue\";\nexport default {\n  setup() {\n    const state = reactive({\n      items: []\n    });\n    onMounted(() => {\n      axios.get(\"/api/shop/korean\").then(response => {\n        state.items = response.data;\n      });\n    });\n    const showModal = korean => {\n      korean.modalVisible = !korean.modalVisible;\n      console.log(korean.storename);\n      console.log(korean.location);\n      console.log(korean.rating);\n      console.log(korean.image);\n    };\n    const setImgUrl = imageName => {\n      return require(`../../../public/${imageName}`);\n    };\n    return {\n      state,\n      showModal,\n      setImgUrl\n    };\n  },\n  data() {\n    return {\n      modalVisible: false\n    };\n  },\n  components: {\n    ModalView,\n    Header\n  },\n  computed: {},\n  methods: {}\n};","map":{"version":3,"names":["reactive","onMounted","axios","Header","ModalView","setup","state","items","get","then","response","data","showModal","korean","modalVisible","console","log","storename","location","rating","image","setImgUrl","imageName","require","components","computed","methods"],"sources":["/Users/soohykim/Desktop/front/src/components/foodCategory/Korean.vue"],"sourcesContent":["<template>\n  <Header />\n  <!--  <h1 v-if=\"modalVisible\">안녕 도완아</h1>-->\n  <div class=\"card shadow-sm\" id=\"cat\">\n    <div class=\"card-body\" >\n      <div><h2 class=\"cattitle\">Korean Food</h2></div>\n      <div class=\"list-group\" id=\"catlist\">\n        <div v-for=\"(korean, idx) in state.items\" :key=\"idx\">\n          <div class=\"list-group-item list-group-item-action\" @click=\"showModal(korean)\">\n            <ModalView v-if=\"korean.modalVisible\">\n              <h1>안녕 도완아</h1>\n              <template v-slot:modalText>\n                <div class=\"card shadow-sm\">\n                  <div class=\"card-body\">\n                    <p class=\"card-text\">\n                      <span><h3 class=\"shopTitle\"> {{korean.storename}} </h3></span><br>\n                      <span class=\"shop1\"> {{korean.category}} </span><br>\n                      <span class = \"shop2\"> {{korean.location}} </span><br>\n                      <span><i class=\"fa fa-star\" aria-hidden=\"true\" style=\"color: yellow\" ></i>  {{korean.rating}} </span><br>\n                      <span><img :src=\"setImgUrl(korean.image)\" /></span>\n                    </p>\n                  </div>\n                </div>\n              </template>\n            </ModalView>\n            <div class=\"card shadow-sm\">\n              <div class=\"card-body\">\n                <p class=\"card-text\">\n                  <span><h3 class=\"shopTitle\"> {{korean.storename}} </h3></span><br>\n                  <span class=\"shop1\"> {{korean.category}} </span><br>\n                  <span class = \"shop2\"> {{korean.location}} </span><br>\n                  <span> {{korean.rating}} </span><br>\n                  <span><img :src=\"setImgUrl(korean.image)\" />{{korean.image}}</span>\n\n                </p>\n              </div>\n            </div>\n          </div>\n        </div> <!-- v-for-->\n      </div>\n\n    </div>\n  </div>\n</template>\n\n<script>\nimport { reactive, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport Header from \"@/components/header/Header.vue\";\nimport ModalView from \"@/components/foodCategory/ModalView.vue\";\n\nexport default {\n  setup() {\n    const state = reactive({\n      items: [],\n    });\n\n    onMounted(() => {\n      axios.get(\"/api/shop/korean\").then((response) => {\n        state.items = response.data;\n      });\n    });\n\n    const showModal = (korean) => {\n      korean.modalVisible = !korean.modalVisible;\n      console.log(korean.storename)\n      console.log(korean.location)\n      console.log(korean.rating)\n      console.log(korean.image)\n    };\n\n    const setImgUrl = (imageName) => {\n      return require(`../../../public/${imageName}`);\n    };\n\n    return { state, showModal, setImgUrl };\n  },\n  data() {\n    return {\n      modalVisible : false\n    }\n  },\n  components: {ModalView, Header},\n  computed: {\n  },\n  methods: {\n  }\n};\n\n</script>\n\n<style scoped>\n.shop2{\n  color : dimgray;\n}\n\n.shopTitle{\n  border : 1px solid yellow;\n  padding : 0;\n}\n\n.shop1{\n  border : 1px solid pink;\n  color : cadetblue;\n}\n#cat{\n  border : 1px solid green;\n  width : 70%;\n  height : auto;\n  margin : auto;\n}\n\n.cattitle{\n  border : 1px solid red;\n  padding : 20px;\n  font-weight: 700;\n}\n\n#catlist{\n  border : 1px solid blue;\n}\n\n</style>"],"mappings":"AA8CA,SAASA,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AACzC,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,gCAAgC;AACnD,OAAOC,SAAQ,MAAO,yCAAyC;AAE/D,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC;MACrBO,KAAK,EAAE;IACT,CAAC,CAAC;IAEFN,SAAS,CAAC,MAAM;MACdC,KAAK,CAACM,GAAG,CAAC,kBAAkB,CAAC,CAACC,IAAI,CAAEC,QAAQ,IAAK;QAC/CJ,KAAK,CAACC,KAAI,GAAIG,QAAQ,CAACC,IAAI;MAC7B,CAAC,CAAC;IACJ,CAAC,CAAC;IAEF,MAAMC,SAAQ,GAAKC,MAAM,IAAK;MAC5BA,MAAM,CAACC,YAAW,GAAI,CAACD,MAAM,CAACC,YAAY;MAC1CC,OAAO,CAACC,GAAG,CAACH,MAAM,CAACI,SAAS;MAC5BF,OAAO,CAACC,GAAG,CAACH,MAAM,CAACK,QAAQ;MAC3BH,OAAO,CAACC,GAAG,CAACH,MAAM,CAACM,MAAM;MACzBJ,OAAO,CAACC,GAAG,CAACH,MAAM,CAACO,KAAK;IAC1B,CAAC;IAED,MAAMC,SAAQ,GAAKC,SAAS,IAAK;MAC/B,OAAOC,OAAO,CAAE,mBAAkBD,SAAU,EAAC,CAAC;IAChD,CAAC;IAED,OAAO;MAAEhB,KAAK;MAAEM,SAAS;MAAES;IAAU,CAAC;EACxC,CAAC;EACDV,IAAIA,CAAA,EAAG;IACL,OAAO;MACLG,YAAW,EAAI;IACjB;EACF,CAAC;EACDU,UAAU,EAAE;IAACpB,SAAS;IAAED;EAAM,CAAC;EAC/BsB,QAAQ,EAAE,CACV,CAAC;EACDC,OAAO,EAAE,CACT;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}