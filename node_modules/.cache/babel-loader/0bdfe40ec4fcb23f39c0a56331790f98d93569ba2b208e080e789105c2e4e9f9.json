B4&key=AIzaSyB21eMRg-uMRk-i7r27DPDkntXbR5_kvnk', {\r\n      headers: {\r\n        'x-cors-api-key': 'temp_924d232f48d59dabc25af9778ab72f25'\r\n      }\r\n    })\r\n        .then(res => res.json())\r\n        .then(res => {\r\n          parseJsonData = res;\r\n          this.jsonData = parseJsonData.results;\r\n          console.log(this.jsonData);\r\n          console.log(this.jsonData[1].name);\r\n        });\r\n  },  // mounted()\r\n  methods: {\r\n    showPlaceInfo: function () {\r\n      // let detailPlaceInfo;\r\n      fetch('https://proxy.cors.sh/https://maps.googleapis.com/maps/api/place/details/json?placeid=ChIJP1CJmGGcfDUR7ffziy-8NZA&key=AIzaSyB21eMRg-uMRk-i7r27DPDkntXbR5_kvnk', {\r\n        headers: {\r\n          'x-cors-api-key': 'temp_924d232f48d59dabc25af9778ab72f25'\r\n        }\r\n      })\r\n          .then(res => res.json())\r\n          .then(res => {\r\n            this.detailPlaceInfo = res.result;\r\n            console.log(this.detailPlaceInfo.name);\r\n            console.log(this.detailPlaceInfo.place_id);\r\n            console.log(this.detailPlaceInfo.geometry.location.lat);\r\n            console.log(this.detailPlaceInfo.geometry.location.lng);\r\n          })\r\n    },  // showPlaceInfo\r\n    setLocation: function () {\r\n\r\n    }\r\n\r\n  },  // method()\r\n})\r\n</script>"],"mappings":"AAmCA,SAAQA,eAAe,QAAO,KAAI;AAClC,SAAQC,SAAS,EAAEC,MAAM,EAAEC,aAAa,EAAEC,UAAU,QAAO,iBAAgB;AAE3E,eAAeJ,eAAe,CAAC;EAC7BK,UAAU,EAAE;IAACJ,SAAS;IAAEC,MAAM;IAAEC,aAAa;IAAEC;EAAU,CAAC;EAC1DE,KAAKA,CAAA,EAAG;IACN,IAAIC,OAAM,GAAI,CAAC;IACf,IAAIC,OAAM,GAAI,CAAC;;IAEf;IACAC,SAAS,CAACC,WAAW,CAACC,kBAAkB,CACnCC,QAAQ,IAAK;MACZ,MAAMC,YAAW,GAAI;QACnBC,kBAAkB,EAAE,IAAI;QAAE;QAC1BC,GAAG,EAAEH,QAAQ,CAACI,MAAM,CAACC,QAAQ;QAC7BC,GAAG,EAAEN,QAAQ,CAACI,MAAM,CAACG;MACvB,CAAC;MAEDZ,OAAM,GAAIM,YAAY,CAACE,GAAG;MAC1BP,OAAM,GAAIK,YAAY,CAACK,GAAG;;MAE1B;MACA;IAEF,CAAC,EACAE,KAAK,IAAK;MACTC,OAAO,CAACC,GAAG,CAAC,wBAAwB,EAAEF,KAAK,CAAC;IAC9C,CACJ;IAEA,MAAMG,MAAK,GAAI;MAACR,GAAG,EAAER,