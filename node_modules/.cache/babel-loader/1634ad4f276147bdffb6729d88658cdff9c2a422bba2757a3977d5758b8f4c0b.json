ton class=\"btn\" @click=\"submit()\">로그인</button> &nbsp;\n      <button class=\"btn\" @click=\"$router.push('/join')\">회원가입</button>\n  </div>\n</template>\n\n<script>\nimport {reactive} from \"vue\";\nimport axios from \"axios\";\nimport store from \"@/scripts/store\";\nimport router from \"@/scripts/router\";\nimport Header from \"@/components/Header.vue\";\n\nexport default {\n  setup() {\n    const state = reactive({\n      form :{\n        email: \"\",\n        password: \"\"\n      }\n    })\n    const submit = () => {\n      axios.post(\"/api/account/login\", state.form).then((res)=> {\n        store.commit(\"setAccount\", res.data); // store 저장\n        console.log(res.data);\n        window.alert(\"로그인 하였습니다\");\n        router.push({path: \"/store\"});\n      })\n    }\n\n    return {state, submit}\n  }\n}\n</script>\n\n<style scoped>\n.form-signin {\n  max-width: 330px;\n  padding: 1rem;\n}\n\n.btn {\n  background-color: #24855b;\n  color: #ffffff;\n}\n\n.form-signin .form-floating:focus-within {\n  z-index: 2;\n}\n\n.form-signin input[type=\"email\"] {\n  margin-bottom: -1px;\n  border-bottom-right-radius: 0;\n  border-bottom-left-radius: 0;\n}\n\n.form-signin input[type=\"password\"] {\n  margin-bottom: 10px;\n  border-top-left-radius: 0;\n  border-top-right-radius: 0;\n}\n</style>"],"mappings":";AA2BA,SAAQA,QAAQ,QAAO,KAAK;AAC5B,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,KAAI,MAAO,iBAAiB;AACnC,OAAOC,MAAK,MAAO,kBAAkB;AACrC,OAAOC,MAAK,MAAO,yBAAyB;AAE5C,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC;MACrBO,IAAG,EAAG;QACJC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACZ;IACF,CAAC;IACD,MAAMC,MAAK,GAAIA,CAAA,KAAM;MACnBT,KAAK,CAACU,IAAI,CAAC,oBAAoB,EAAEL,KAAK,CAACC,IAAI,CAAC,CAACK,IAAI,CAAEC,GAAG,IAAI;QACxDX,KAAK,CAACY,MAAM,CAAC,YAAY,EAAED,GAAG,CAACE,IAAI,CAAC,EAAE;QACtCC,OAAO,CAACC,GAAG,CAACJ,GAAG,CAACE,IAAI,CAAC;QACrBG,MAAM,CAACC,KAAK,CAAC,WAAW,CAAC;QACzBhB,MAAM,CAACiB,IAAI,CAAC;UAACC,IAAI,EAAE;QAAQ,CAAC,CAAC;MAC/B,CAAC;IACH;IAEA,OAAO;MAACf,KAAK;MAAEI;IAAM;EACvB;AACF