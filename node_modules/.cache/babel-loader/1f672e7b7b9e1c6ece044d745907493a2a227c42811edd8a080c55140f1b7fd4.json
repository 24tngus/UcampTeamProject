os\";\nimport {reactive} from \"vue\";\nexport default {\n  name: \"Store\",\n  setup() {\n    const state = reactive({\n      menus: []\n    })\n    axios.get(\"/api/menu\").then(({data}) => {\n      state.menus = data;\n    })\n    return {state};\n  },\n  methods: {\n\n  }\n\n}\n</script>\n\n<style scoped>\n.card .img{\n  display: inline-block;\n  width: 100%;\n  hieght: 250px;\n  background-size: cover;\n  background-position: center;\n}\n\n.btn {\n  width:80px;\n  float: right;\n}\n\n.card .card-body .price{\n  text-decoration: line-through;\n}\n</style>"],"mappings":"AAsCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAAQC,QAAQ,QAAO,KAAK;AAC5B,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,CAAC;MACrBI,KAAK,EAAE;IACT,CAAC;IACDL,KAAK,CAACM,GAAG,CAAC,WAAW,CAAC,CAACC,IAAI,CAAC,CAAC;MAACC;IAAI,CAAC,KAAK;MACtCJ,KAAK,CAACC,KAAI,GAAIG,IAAI;IACpB,CAAC;IACD,OAAO;MAACJ;IAAK,CAAC;EAChB,CAAC;EACDK,OAAO,EAAE,CAET;AAEF"},"metadata":{},"sourceType":"module","externalDependencies":[]}