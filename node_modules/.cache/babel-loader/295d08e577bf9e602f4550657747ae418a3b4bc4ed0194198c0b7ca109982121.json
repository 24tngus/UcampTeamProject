{"ast":null,"code":"import { defineComponent } from 'vue';\nimport { GoogleMap, Marker, MarkerCluster, InfoWindow } from 'vue3-google-map';\nexport default defineComponent({\n  components: {\n    GoogleMap,\n    Marker,\n    MarkerCluster,\n    InfoWindow\n  },\n  setup() {\n    const center = {\n      lat: 37.561736,\n      lng: 126.835419\n    };\n    const locations = [{\n      lat: 37.5593773,\n      lng: 126.832661\n    }, {\n      lat: 37.55795,\n      lng: 126.8397055\n    }];\n    return {\n      center,\n      locations\n    };\n  },\n  data() {\n    return {\n      jsonData: null\n    };\n  },\n  methods: {\n    test: function () {\n      alert(\"test\");\n    },\n    fetchJSONData() {\n      const url = \"https://maps.googleapis.com/maps/api/js?key=API_KEY_VALUE&callback=initGoogleMaps&libraries=places&v=weekly\";\n      fetch(url).then(response => {\n        if (!response.ok) {\n          throw new Error(\"Network response was not ok\");\n        }\n        return response.json();\n      }).then(data => {\n        this.jsonData = data;\n      }).catch(error => {\n        console.error(\"Error fetching JSON data:\", error);\n      });\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","GoogleMap","Marker","MarkerCluster","InfoWindow","components","setup","center","lat","lng","locations","data","jsonData","methods","test","alert","fetchJSONData","url","fetch","then","response","ok","Error","json","catch","error","console"],"sources":["C:\\Ucamp\\VeganProject\\VeganLoadMap\\src\\components\\Map.vue"],"sourcesContent":["<template>\r\n  <GoogleMap\r\n      api-key=\"AIzaSyB21eMRg-uMRk-i7r27DPDkntXbR5_kvnk\"\r\n      style=\"width: 100%; height: 500px\"\r\n      :center=\"center\"\r\n      :zoom=\"15\"\r\n      language=\"kor\"\r\n  >\r\n    <MarkerCluster>\r\n      <Marker v-for=\"(location, i) in locations\" :options=\"{ position: location }\" :key=\"i\"\r\n              @click=\"test()\">\r\n        <InfoWindow>  <!-- Marker 클릭하면 나오는 부분 -->\r\n          <div id=\"contet\">\r\n            <div id=\"siteNotice\"></div>\r\n            <h1 id=\"firstHeading\" class=\"firstHeading\">식당명</h1>\r\n            <div id=\"bodyContent\">\r\n              <p>\r\n                식당 설명\r\n              </p>\r\n              <p>\r\n                Attribution: Uluru,\r\n                <a href=\"https://en.wikipedia.org/w/index.php?title=Uluru&oldid=297882194\">\r\n                  https://en.wikipedia.org/w/index.php?title=Uluru</a\r\n                >\r\n                <img src=\"../../public/img/bread.jpg\" style=\"width: 100px\">\r\n                (last visited June 22, 2009).\r\n              </p>\r\n            </div>\r\n            <div>\r\n              <button @click=\"fetchJSONData()\">Fetch JSON Data</button>\r\n              <div v-if=\"jsonData\">\r\n                <h2>JSON Data:</h2>\r\n                <ul>\r\n                  <li v-for=\"result in jsonData.results\" :key=\"result.name\">\r\n                    <strong>{{ result.name }}</strong> ({{ result.business_status }})\r\n                    <br>\r\n                    Latitude: {{ result.geometry.location.lat }}, Longitude: {{ result.geometry.location.lng }}\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </InfoWindow>\r\n      </Marker>\r\n    </MarkerCluster>\r\n  </GoogleMap>\r\n</template>\r\n\r\n<script>\r\nimport {defineComponent} from 'vue'\r\nimport {GoogleMap, Marker, MarkerCluster, InfoWindow} from 'vue3-google-map'\r\n\r\nexport default defineComponent({\r\n  components: {GoogleMap, Marker, MarkerCluster, InfoWindow},\r\n  setup() {\r\n    const center = {lat: 37.561736, lng: 126.835419}\r\n\r\n    const locations = [\r\n      {lat: 37.5593773, lng: 126.832661},\r\n      {lat: 37.55795, lng: 126.8397055},\r\n    ]\r\n\r\n    return {center, locations}\r\n  },\r\n  data() {\r\n    return {\r\n      jsonData : null,\r\n    };\r\n  },\r\n  methods: {\r\n    test: function () {\r\n      alert(\"test\");\r\n    },\r\n    fetchJSONData() {\r\n      const url = \"https://maps.googleapis.com/maps/api/js?key=API_KEY_VALUE&callback=initGoogleMaps&libraries=places&v=weekly\";\r\n\r\n      fetch(url)\r\n          .then((response) => {\r\n            if (!response.ok) {\r\n              throw new Error(\"Network response was not ok\");\r\n            }\r\n            return response.json();\r\n          })\r\n          .then((data) => {\r\n            this.jsonData = data;\r\n          })\r\n          .catch((error) => {\r\n            console.error(\"Error fetching JSON data:\", error);\r\n          });\r\n    },\r\n  }\r\n})\r\n</script>"],"mappings":"AAiDA,SAAQA,eAAe,QAAO,KAAI;AAClC,SAAQC,SAAS,EAAEC,MAAM,EAAEC,aAAa,EAAEC,UAAU,QAAO,iBAAgB;AAE3E,eAAeJ,eAAe,CAAC;EAC7BK,UAAU,EAAE;IAACJ,SAAS;IAAEC,MAAM;IAAEC,aAAa;IAAEC;EAAU,CAAC;EAC1DE,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAI;MAACC,GAAG,EAAE,SAAS;MAAEC,GAAG,EAAE;IAAU;IAE/C,MAAMC,SAAQ,GAAI,CAChB;MAACF,GAAG,EAAE,UAAU;MAAEC,GAAG,EAAE;IAAU,CAAC,EAClC;MAACD,GAAG,EAAE,QAAQ;MAAEC,GAAG,EAAE;IAAW,CAAC,CACnC;IAEA,OAAO;MAACF,MAAM;MAAEG;IAAS;EAC3B,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAO,EAAI;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,IAAI,EAAE,SAAAA,CAAA,EAAY;MAChBC,KAAK,CAAC,MAAM,CAAC;IACf,CAAC;IACDC,aAAaA,CAAA,EAAG;MACd,MAAMC,GAAE,GAAI,6GAA6G;MAEzHC,KAAK,CAACD,GAAG,EACJE,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACA,QAAQ,CAACC,EAAE,EAAE;UAChB,MAAM,IAAIC,KAAK,CAAC,6BAA6B,CAAC;QAChD;QACA,OAAOF,QAAQ,CAACG,IAAI,CAAC,CAAC;MACxB,CAAC,EACAJ,IAAI,CAAER,IAAI,IAAK;QACd,IAAI,CAACC,QAAO,GAAID,IAAI;MACtB,CAAC,EACAa,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACD,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;MACnD,CAAC,CAAC;IACR;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}