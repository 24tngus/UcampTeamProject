{"ast":null,"code":"import { reactive } from \"vue\";\nimport axios from \"axios\";\nimport lib from \"@/scripts/lib\";\nimport Header from \"@/components/header/Header.vue\";\nexport default {\n  components: {\n    Header\n  },\n  setup() {\n    const state = reactive({\n      items: []\n    });\n    const load = () => {\n      axios.get(\"/api/cart/shop\").then(({\n        data\n      }) => {\n        state.items = data;\n      });\n    };\n    const remove = shopSeq => {\n      axios.delete(`/api/cart/shop/${shopSeq}`).then(() => {\n        load();\n      });\n    };\n    load();\n    return {\n      state,\n      lib,\n      remove\n    };\n  }\n};","map":{"version":3,"names":["reactive","axios","lib","Header","components","setup","state","items","load","get","then","data","remove","shopSeq","delete"],"sources":["C:\\VeganLoadMap\\src\\components\\user\\Cart.vue"],"sourcesContent":["<template>\r\n  <Header/>\r\n  <div class=\"cart\">\r\n    <div class=\"container\">\r\n      <ul>\r\n        <li v-for=\"(shop, idx) in state.items\" :key=\"idx\">\r\n          <img :src=\"shop.image\"/>\r\n          <span class=\"name\">{{ shop.storename }}</span>\r\n          <span class=\"category\">{{shop.category}}</span>\r\n          <span class=\"category\">예약금{{ lib.getNumberFormatted(10000) }}원</span>\r\n          <i class=\"fa fa-trash\" @click=\"remove(shop.seq)\"></i>\r\n        </li>\r\n      </ul>\r\n      <router-link to=\"/reserve\" class=\"btn btn-primary\">예약하기</router-link>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {reactive} from \"vue\";\r\nimport axios from \"axios\";\r\nimport lib from \"@/scripts/lib\";\r\nimport Header from \"@/components/header/Header.vue\";\r\n\r\nexport default {\r\n  components: {Header},\r\n  setup() {\r\n    const state = reactive({\r\n      items: []\r\n    })\r\n\r\n    const load = () => {\r\n      axios.get(\"/api/cart/shop\").then(({data}) => {\r\n        state.items = data;\r\n      })\r\n    };\r\n\r\n    const remove = (shopSeq) => {\r\n      axios.delete(`/api/cart/shop/${shopSeq}`).then(() => {\r\n        load();\r\n      })\r\n    }\r\n\r\n    load();\r\n\r\n    return {state, lib, remove}\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.cart ul {\r\n  list-style: none;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.cart ul li {\r\n  border: 1px solid #eee;\r\n  margin-top: 25px;\r\n  margin-bottom: 25px;\r\n}\r\n\r\n.cart ul li img {\r\n  width: 150px;\r\n  height: 150px;\r\n}\r\n\r\n.cart ul li .name {\r\n  margin-left: 25px;\r\n}\r\n\r\n.cart ul li .category {\r\n  margin-left: 25px;\r\n}\r\n\r\n.cart ul li i {\r\n  float: right;\r\n  font-size: 20px;\r\n  margin-top: 65px;\r\n  margin-right: 50px;\r\n}\r\n\r\n.cart .btn {\r\n  width:300px;\r\n  display:block;\r\n  margin:0 auto;\r\n  padding:30px 50px;\r\n  font-size: 20px;\r\n}\r\n</style>"],"mappings":"AAmBA,SAAQA,QAAQ,QAAO,KAAK;AAC5B,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,GAAE,MAAO,eAAe;AAC/B,OAAOC,MAAK,MAAO,gCAAgC;AAEnD,eAAe;EACbC,UAAU,EAAE;IAACD;EAAM,CAAC;EACpBE,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC;MACrBO,KAAK,EAAE;IACT,CAAC;IAED,MAAMC,IAAG,GAAIA,CAAA,KAAM;MACjBP,KAAK,CAACQ,GAAG,CAAC,gBAAgB,CAAC,CAACC,IAAI,CAAC,CAAC;QAACC;MAAI,CAAC,KAAK;QAC3CL,KAAK,CAACC,KAAI,GAAII,IAAI;MACpB,CAAC;IACH,CAAC;IAED,MAAMC,MAAK,GAAKC,OAAO,IAAK;MAC1BZ,KAAK,CAACa,MAAM,CAAE,kBAAiBD,OAAQ,EAAC,CAAC,CAACH,IAAI,CAAC,MAAM;QACnDF,IAAI,CAAC,CAAC;MACR,CAAC;IACH;IAEAA,IAAI,CAAC,CAAC;IAEN,OAAO;MAACF,KAAK;MAAEJ,GAAG;MAAEU;IAAM;EAC5B;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}