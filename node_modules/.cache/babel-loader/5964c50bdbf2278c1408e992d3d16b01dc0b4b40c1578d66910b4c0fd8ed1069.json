nt-family: \"Raleway\", sans-serif;\n  font-size: 0.9rem;\n}\n\n.coming-soon {\n  position: relative;\n  font-family: \"Raleway\", sans-serif;\n  font-weight: 300;\n  font-size: 1rem;\n  padding: 10px;\n  padding-left: 0px;\n  text-transform: uppercase;\n}\n.beginner:after {\n  content: \"\\24B6\\FE0E\";\n  position: absolute;\n  font-size: 28px;\n  right: 5px;\n}\n.medium:after {\n  content: \"\\24C2\\FE0E\";\n  position: absolute;\n  font-size: 28px;\n  right: 5px;\n}\n.advanced:after {\n  content: \"\\24BB\\FE0E\";\n  position: absolute;\n  font-size: 28px;\n  right: 5px;\n}\n\n.timetable .purple {\n  background: #bd70bdff;\n}\n.timetable .orange {\n  background: #f8a628ff;\n}\n.timetable .orange-medium {\n  background: #f58c1eff;\n}\n.timetable .green {\n  background: #57bf33ff;\n}\n.timetable .green-light {\n  background: #97d99eff;\n}\n.timetable .cyan {\n  background: #78d1deff;\n}\n.timetable .orange-dark {\n  background: #f36b35ff;\n}\n.timetable .pink {\n  background: #f03a4fff;\n}\n\n\n\n@media only screen and (min-width: 480px) and (max-width: 1199px) {\n  .timetable {\n    display: grid;\n    grid-template-columns: repeat(auto-fit, minmax(/*330 */210px, 1fr) );\n    grid-auto-rows: minmax( min-content, 1fr);\n    grid-gap: 5px;\n  }\n  .timetable .day-title {\n    border: none;\n    align-items: center;\n    grid-column-start: 1;\n    grid-column-end: -1; }\n  /*\n  .class-title {\n    font-size: 3rem;\n\n  }\n  .class-title-sm {\n    font-size: 2.5rem;\n  }\n  .class-time {\n    font-size: 2.5rem;\n    font-weight: 600;\n  }\n  .class-teacher,\n  .coming-soon {\n    font-size: 2rem;\n    font-weight: 600;\n  } */\n  .beginner:after,\n  .medium:after,\n  .advanced:after {\n    font-size: 2.8rem;\n  }\n  .timetable>li {\n  }\n  .timetable > li abbr {\n    font-size: 0;\n  }\n  .timetable > li abbr:after {\n    content: attr(title);\n    font-size: 4rem;\n  }\n\n  .timetable > li:before {\n    font-size: 1rem;\n  }\n  .timetable > div {\n    display: none