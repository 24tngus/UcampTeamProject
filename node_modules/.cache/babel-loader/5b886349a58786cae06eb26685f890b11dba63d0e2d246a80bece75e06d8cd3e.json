{"ast":null,"code":"import axios from \"axios\";\nimport { reactive } from \"vue\";\nimport Header from \"@/components/header/Header.vue\";\nexport default {\n  name: \"Shop\",\n  components: {\n    Header\n  },\n  setup() {\n    const state = reactive({\n      items: []\n    });\n    axios.get(\"/api/shop\").then(({\n      data\n    }) => {\n      state.items = data;\n    });\n    const addToCart = shopSeq => {\n      axios.post(`/api/cart/shop/${shopSeq}`).then(() => {\n        console.log('success');\n      });\n    };\n    return {\n      state,\n      addToCart\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","Header","name","components","setup","state","items","get","then","data","addToCart","shopSeq","post","console","log"],"sources":["C:\\VeganLoadMap\\src\\pages\\Shop.vue"],"sourcesContent":["<template>\r\n  <Header/>\r\n  <div class=\"shop\">\r\n    <div class=\"album py-5 bg-body-tertiary\">\r\n      <div class=\"container\">\r\n\r\n        <div class=\"row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3\">\r\n          <div class=\"col\" v-for=\"(shop, idx) in state.items\" :key=\"idx\">\r\n            <div class=\"card shadow-sm\">\r\n<!--              <div :style=\"`background-image : url(${shop.image})`\"></div>-->\r\n              <img :src=\"shop.image\"/>\r\n              <div class=\"card-body\">\r\n                <p class=\"card-text\">\r\n                  <span> {{shop.storename}} </span> &nbsp;\r\n                </p>\r\n                <div class=\"d-flex justify-content-between align-items-center\">\r\n                  <button class=\"btn btn-primary\" @click=\"$router.push('/menu')\">\r\n                    <i class=\"fa fa-plus\" aria-hidden=\"true\"></i>\r\n                  </button>\r\n                  <button class=\"btn btn-primary\" @click=\"addToCart(shop.seq)\">\r\n                    <i class=\"fa fa-shopping-cart\" aria-hidden=\"true\"></i>\r\n                  </button>\r\n                  <button class=\"btn btn-primary\" @click=\"$router.push('/reserve')\">\r\n                    <i class=\"fa fa-calendar-check-o\" aria-hidden=\"true\"></i>\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport {reactive} from \"vue\";\r\nimport Header from \"@/components/header/Header.vue\";\r\n\r\nexport default {\r\n  name: \"Shop\",\r\n  components: {Header},\r\n  setup() {\r\n    const state = reactive({\r\n      items: []\r\n    })\r\n    axios.get(\"/api/shop\").then(({data}) => {\r\n      state.items = data;\r\n    })\r\n\r\n    const addToCart = (shopSeq) => {\r\n      axios.post(`/api/cart/shop/${shopSeq}`).then(() => {\r\n        console.log('success')\r\n      })\r\n    };\r\n\r\n    return {state, addToCart};\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.card .img{\r\n  display: inline-block;\r\n  width: 100%;\r\n  hieght: 250px;\r\n  background-size: cover;\r\n  background-position: center;\r\n}\r\n\r\n.btn {\r\n  width:80px;\r\n  float: right;\r\n}\r\n\r\n.card .card-body .price{\r\n  text-decoration: line-through;\r\n}\r\n</style>"],"mappings":"AAsCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAAQC,QAAQ,QAAO,KAAK;AAC5B,OAAOC,MAAK,MAAO,gCAAgC;AAEnD,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IAACF;EAAM,CAAC;EACpBG,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,CAAC;MACrBM,KAAK,EAAE;IACT,CAAC;IACDP,KAAK,CAACQ,GAAG,CAAC,WAAW,CAAC,CAACC,IAAI,CAAC,CAAC;MAACC;IAAI,CAAC,KAAK;MACtCJ,KAAK,CAACC,KAAI,GAAIG,IAAI;IACpB,CAAC;IAED,MAAMC,SAAQ,GAAKC,OAAO,IAAK;MAC7BZ,KAAK,CAACa,IAAI,CAAE,kBAAiBD,OAAQ,EAAC,CAAC,CAACH,IAAI,CAAC,MAAM;QACjDK,OAAO,CAACC,GAAG,CAAC,SAAS;MACvB,CAAC;IACH,CAAC;IAED,OAAO;MAACT,KAAK;MAAEK;IAAS,CAAC;EAC3B;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}