"state.form.category\" />\n                <label class=\"fieldlabels\">Í∞ÄÍ≤å ÏúÑÏπò : *</label>\n                <input type=\"text\" placeholder=\"Ï£ºÏÜå\" id=\"location\" v-model=\"state.form.location\" style=\"height: 100px;\"/>\n                <label class=\"fieldlabels\">Í∞ÄÍ≤å Ï†ÑÌôîÎ≤àÌò∏ : *</label>\n                <input type=\"text\" placeholder=\"Í∞ÄÍ≤å Ï†ÑÌôîÎ≤àÌò∏\" id=\"phonenumber\" v-model=\"state.form.phonenumber\" />\n              </div> <input type=\"button\" @click=\"next\" name=\"next\" class=\"action-button\" value=\"Next\" />\n              <input type=\"button\" @click=\"previous\" name=\"previous\" class=\"previous action-button-previous\" value=\"Previous\" />\n            </fieldset>\n\n            <fieldset v-if=\"this.flag === 3\">\n              <ul id=\"progressbar\">\n                <li class=\"active\" id=\"account\"><strong>Name</strong></li>\n                <li id=\"personal\"><strong>Information</strong></li>\n                <li id=\"payment\"><strong>Image</strong></li>\n                <li id=\"confirm\"><strong>Menu</strong></li>\n              </ul><br>\n              <div class=\"form-card\">\n                <div class=\"row\">\n                  <div class=\"col-7\">\n                    <h2 class=\"fs-title\">Image Upload:</h2>\n                  </div><br>\n                  <div class=\"col-5\">\n                    <h2 class=\"steps\">3/4</h2>\n                  </div>\n                </div>\n                <label class=\"fieldlabels\">Í∞ÄÍ≤å Ïù¥ÎØ∏ÏßÄ Ï≤®Î∂Ä :</label>\n                <input type=\"file\" ref=\"fileInput\" accept=\"image/*\" @change=\"onFileChange\" />\n                <img v-if=\"imageURL !== 0\"  :src=\"imageURL\" style=\"width: 100%\"/>\n              </div> <input type=\"button\" @click=\"next\" name=\"next\" class=\"action-button\" value=\"Submit\" />\n              <input type=\"button\" @click=\"previous\" name=\"previous\" class=\"previous action-button-previous\" value=\"Previous\" /