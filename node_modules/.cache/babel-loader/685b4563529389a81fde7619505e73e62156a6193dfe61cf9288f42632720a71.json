lag + 1;\n        }\n      }\n    },\n    previous() {\n      if (this.flag > 1) {\n        this.flag--;\n        console.log(\"prev flag\", this.flag);\n      }\n    }\n  },\n  data() {\n    return {\n      selectedFile: null,\n      imageURL: \"\",\n      flag: 1\n    };\n  },\n  setup() {\n    const state = reactive({\n      flag: 0,\n      // i : 0,\n      form: {\n        storename: \"\",\n        detailinfo: \"\",\n        category: \"\",\n        location: \"\",\n        phonenumber: \"\",\n        image: \"\"\n      },\n      reg: {\n        shopseq: 0,\n        menu: \"\",\n        info: \"\",\n        price: 0,\n        image: \"\",\n        sale: 0\n      }\n    });\n    const exist = exist => {\n      if (state.form.storename == \"\") {\n        window.alert(\"Í∞ÄÍ≤å Ïù¥Î¶ÑÏùÑ ÏûÖÎ†•Ìï¥Ï£ºÏÑ∏Ïöî\");\n      } else {\n        axios.get(`/api/shop/register/join/${exist}`).then(res => {\n          if (res.data == 0) {\n            window.alert(\"Ï§ëÎ≥µÎêú Í∞ÄÍ≤å Ïù¥Î¶ÑÏûÖÎãàÎã§. ÏßÄÏ†êÎ™ÖÏùÑ Î∂ôÏó¨ÏÑú Îã§Ïãú ÏûÖÎ†•Ìï¥Ï£ºÏÑ∏Ïöî\");\n          } else {\n            window.alert(\"Îì±Î°ù Í∞ÄÎä•Ìïú Í∞ÄÍ≤å ÏûÖÎãàÎã§\");\n            state.flag = 1;\n          }\n        });\n      }\n    };\n    const register = () => {\n      axios.post(\"/api/shop/register/join\", state.form).then(() => {\n        // while (state.i == 5) {\n        //   menu();\n        //   i++;\n        // };\n        // window.alert(\"Í∞ÄÍ≤åÍ∞Ä Îì±Î°ù ÎêòÏóàÏäµÎãàÎã§\");\n        // router.push({path: \"/home_store\"});\n      });\n    };\n    const menu = () => {\n      const register = {\n        shopseq: this.state.form.seq,\n        menu: state.reg.menu,\n        info: state.reg.info,\n        price: state.reg.price,\n        image: state.reg.image,\n        sale: state.reg.sale\n      };\n      axios.post(\"/api/menu/register\", register).then(() => {\n        // console.log(\"window\", state.i);\n        console.log(\"ÏàòÌñâ ÏôÑÎ£å\");\n      });\n    };\n    return {\n      state,\n      register,\n      exist\n    };\n  }\n};","map":{"version":3,"names":["Header2","axios","reactive","router","components","methods","onFileChange","selectedFile","$refs","fileInput","files","formData","FormData","append","post","headers","then","data","state","form","image","downloadImage","fileId","imageURL","next","flag","storename","window","alert","detailinfo","category","location","phonenumber","previous","console","log","setup","reg","shopseq","menu","info","price","sale","exist","get","res","register","seq"],"sources":["/Users/soohykim/Desktop/front/src/components/shop/ShopInsert2.vue"],"sourcesContent":["<template>\n  <Header2 />\n  <section class=\"showcase-area\" id=\"showcase\">\n    <div class=\"background-image\" :style=\"'background-image: url(/aaa1.png)'\"></div>\n    <div class=\"showcase-container\">\n      <div class=\"main-title\" id=\"home\">Store registration</div><br>\n<!--      <p class=\"p-title\">ÎÇ¥ Í∞ÄÍ≤å Îì±Î°ù</p>-->\n<!--      <a href=\"/shop_insert2\" class=\"btn btn-primary\">ÏãúÏûëÌïòÍ∏∞</a>-->\n    </div>\n  </section>\n  <div class=\"container-fluid\">\n    <div class=\"row justify-content-center\">\n      <div class=\"col-11 col-sm-10 col-md-10 col-lg-6 col-xl-5 text-center p-0 mt-3 mb-2\">\n        <div class=\"card px-0 pt-4 pb-0 mt-3 mb-3\">\n          <h2 id=\"heading\">Í∞ÄÍ≤å Îì±Î°ù</h2><br><br>\n          <p>Í∞ÄÍ≤åÎ•º Îì±Î°ùÌïòÎ©¥ VeganLoadMapÏóêÏÑú ÏòàÏïΩ ÏÑúÎπÑÏä§Î•º Ïù¥Ïö©ÌïòÏã§ Ïàò ÏûàÏäµÎãàÎã§.</p><br><br>\n          <div id=\"msform\">\n\n            <!-- fieldsets -->\n            <fieldset v-if=\"flag === 1\">\n              <ul id=\"progressbar\">\n                <li class=\"active\" id=\"account\"><strong>Name</strong></li>\n                <li id=\"personal\"><strong>Information</strong></li>\n                <li id=\"payment\"><strong>Image</strong></li>\n                <li id=\"confirm\"><strong>Menu</strong></li>\n              </ul><br>\n              <div class=\"form-card\">\n                <div class=\