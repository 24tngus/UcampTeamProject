Rk-i7r27DPDkntXbR5_kvnk', {\r\n        headers: {\r\n          'x-cors-api-key': 'temp_924d232f48d59dabc25af9778ab72f25'\r\n        }\r\n      })\r\n          .then(res => res.json())\r\n          .then(res => {\r\n            this.detailPlaceInfo = res.result;\r\n            console.log(this.detailPlaceInfo.name);\r\n            console.log(this.detailPlaceInfo.place_id);\r\n            console.log(this.detailPlaceInfo.geometry.location.lat);\r\n            console.log(this.detailPlaceInfo.geometry.location.lng);\r\n          })\r\n    },  // showPlaceInfo\r\n    getUserLocation: function () {\r\n      // User의 현재 위치\r\n      navigator.geolocation.getCurrentPosition(\r\n          (position) => {\r\n            const userLocation = {\r\n              enableHighAccuracy: true, // 정확도 향상\r\n              lat: position.coords.latitude,\r\n              lng: position.coords.longitude,\r\n            };\r\n\r\n            var userLat = userLocation.lat;\r\n            var userLng = userLocation.lng;\r\n\r\n            // userLat = parseFloat(userLocation.lat);\r\n            // userLng = parseFloat(userLocation.lng);\r\n\r\n          },\r\n          (error) => {\r\n            console.log(\"Error User Location : \", error);\r\n          }\r\n      )\r\n    }\r\n\r\n  },  // method()\r\n})\r\n</script>"],"mappings":"AAmCA,SAAQA,eAAe,QAAO,KAAI;AAClC,SAAQC,SAAS,EAAEC,MAAM,EAAEC,aAAa,EAAEC,UAAU,QAAO,iBAAgB;AAE3E,eAAeJ,eAAe,CAAC;EAC7BK,UAAU,EAAE;IAACJ,SAAS;IAAEC,MAAM;IAAEC,aAAa;IAAEC;EAAU,CAAC;EAC1DE,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAI;MAACC,GAAG,EAAEC,OAAO;MAAEC,GAAG,EAAEC;IAAO;IAE1C,MAAMC,SAAQ,GAAI,CAChB;MAACJ,GAAG,EAAE,UAAU;MAAEE,GAAG,EAAE;IAAU,CAAC,EAClC;MAACF,GAAG,EAAE,QAAQ;MAAEE,GAAG,EAAE;IAAW,CAAC,CACnC;IAEA,OAAO;MAACH,MAAM;MAAEK;IAAS;EAC3B,CAAC;EAAG;EACJC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAO,EAAI,IAAI;MACfC,eAAc,EAAI;IACpB,CAAC;EACH,CAAC;EAAG;EACJC,OAAOA,CAAA,EAAG;IACR,IAAIC,aAAa;IAIjBC,KAAK,CAAC,kNAAkN,EAAE;MACxNC,OAAO,EAAE;QACP,gBAAgB,EAAE;MACpB;IACF,CAAC,EACIC,IAA