{"ast":null,"code":"import { reactive } from \"vue\";\nimport axios from \"axios\";\nimport lib from \"@/scripts/lib\";\nimport Header from \"@/components/header/Header.vue\";\nexport default {\n  components: {\n    Header\n  },\n  setup() {\n    const state = reactive({\n      items: []\n    });\n    const load = () => {\n      axios.get(\"/api/cart/shop\").then(({\n        data\n      }) => {\n        console.log(\"load\");\n        state.items = data;\n      });\n    };\n    const remove = shopSeq => {\n      axios.delete(`/api/cart/delete/${shopSeq}`).then(() => {\n        load();\n        window.alert(\"삭제되었습니다\");\n      });\n    };\n    load();\n    return {\n      state,\n      lib,\n      remove\n    };\n  }\n};","map":{"version":3,"names":["reactive","axios","lib","Header","components","setup","state","items","load","get","then","data","console","log","remove","shopSeq","delete","window","alert"],"sources":["/Users/soohykim/Desktop/front/src/components/user/Cart.vue"],"sourcesContent":["<template>\n  <Header/>\n  <div class=\"container\">\n    <figure class=\"snip1423\" v-for=\"(shop, idx) in state.items\" :key=\"idx\">\n      <img :src=\"shop.image\"/>\n<!--      <img src=\"https://s3-us-west-2.amazonaws.com/s.cdpn.io/331810/sample57.jpg\" alt=\"sample57\" />-->\n      <figcaption>\n        <h3>{{ shop.storename }}</h3>\n        <p>{{shop.location}}</p>\n<!--        <div class=\"price\">-->\n<!--          예약금{{ lib.getNumberFormatted(10000) }}원-->\n<!--        </div>-->\n        <i class=\"fa fa-trash-o\" aria-hidden=\"true\" @click=\"remove(shop.seq)\"></i>\n      </figcaption>\n<!--      <a href=\"#\"></a>-->\n    </figure>\n  </div>\n</template>\n\n<script>\nimport {reactive} from \"vue\";\nimport axios from \"axios\";\nimport lib from \"@/scripts/lib\";\nimport Header from \"@/components/header/Header.vue\";\n\nexport default {\n  components: {Header},\n  setup() {\n    const state = reactive({\n      items: []\n    })\n\n    const load = () => {\n      axios.get(\"/api/cart/shop\").then(({data}) => {\n        console.log(\"load\");\n        state.items = data;\n      })\n    };\n\n    const remove = (shopSeq) => {\n      axios.delete(`/api/cart/delete/${shopSeq}`).then(() => {\n        load();\n        window.alert(\"삭제되었습니다\");\n      })\n    }\n\n    load();\n\n    return {state, lib, remove}\n  }\n}\n</script>\n\n<style scoped>\n@import url(https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css);\n@import url(https://fonts.googleapis.com/css?family=Oswald:300,400);\n\n.container {\n  display: flex;\n  flex-wrap: wrap; /* 자동 줄 바꿈을 위해 설정 */\n  justify-content: space-between; /* figure 요소들을 가로로 균등하게 배치 */\n  margin: 10px;\n}\n\n.snip1423 {\n  font-family: 'Oswald', Arial, sans-serif;\n  position: relative;\n  margin: 10px;\n  min-width: 230px;\n  max-width: 315px;\n  width: 100%;\n  background: #ffffff;\n  text-align: center;\n  color: #000000;\n  box-shadow: 0 0 5px rgba(0, 0, 0, 0.15);\n  font-size: 16px;\n  padding: 15px;\n  -webkit-box-sizing: border-box;\n  box-sizing: border-box;\n  -webkit-transition: all 0.2s ease-out;\n  transition: all 0.2s ease-out;\n}\n.snip1423 * {\n  -webkit-box-sizing: padding-box;\n  -webkit-transition: all 0.2s ease-out;\n  transition: all 0.2s ease-out;\n}\n.snip1423 img {\n  max-width: 100%;\n  vertical-align: top;\n  position: relative;\n  width: 100%; /* 이미지 너비를 100%로 설정하여 부모 요소에 맞춥니다 */\n  height: 100%; /* 이미지 높이를 100%로 설정하여 부모 요소에 맞춥니다 */\n  object-fit: cover;\n}\n.snip1423 figcaption {\n  padding: 20px 15px;\n}\n.snip1423 h3,\n.snip1423 p {\n  margin: 0;\n}\n.snip1423 h3 {\n  font-size: 1.3em;\n  font-weight: 400;\n  margin-bottom: 5px;\n  text-transform: uppercase;\n}\n.snip1423 p {\n  font-size: 0.9em;\n  letter-spacing: 1px;\n  font-weight: 300;\n}\n.snip1423 .price {\n  font-weight: 500;\n  font-size: 1.4em;\n  line-height: 48px;\n  letter-spacing: 1px;\n}\n.snip1423 .price s {\n  margin-right: 5px;\n  opacity: 0.5;\n  font-size: 0.9em;\n}\n.snip1423 i {\n  position: absolute;\n  bottom: 0;\n  left: 50%;\n  -webkit-transform: translate(-50%, 50%);\n  transform: translate(-50%, 50%);\n  width: 56px;\n  line-height: 56px;\n  text-align: center;\n  border-radius: 50%;\n  background-color: #666666;\n  color: #ffffff;\n  font-size: 1.6em;\n  border: 4px solid #ffffff;\n}\n.snip1423 a {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  z-index: 1;\n}\n.snip1423:hover,\n.snip1423.hover {\n  -webkit-transform: translateY(-5px);\n  transform: translateY(-5px);\n}\n.snip1423:hover i,\n.snip1423.hover i {\n  background-color: #2980b9;\n}\n</style>"],"mappings":"AAoBA,SAAQA,QAAQ,QAAO,KAAK;AAC5B,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,GAAE,MAAO,eAAe;AAC/B,OAAOC,MAAK,MAAO,gCAAgC;AAEnD,eAAe;EACbC,UAAU,EAAE;IAACD;EAAM,CAAC;EACpBE,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC;MACrBO,KAAK,EAAE;IACT,CAAC;IAED,MAAMC,IAAG,GAAIA,CAAA,KAAM;MACjBP,KAAK,CAACQ,GAAG,CAAC,gBAAgB,CAAC,CAACC,IAAI,CAAC,CAAC;QAACC;MAAI,CAAC,KAAK;QAC3CC,OAAO,CAACC,GAAG,CAAC,MAAM,CAAC;QACnBP,KAAK,CAACC,KAAI,GAAII,IAAI;MACpB,CAAC;IACH,CAAC;IAED,MAAMG,MAAK,GAAKC,OAAO,IAAK;MAC1Bd,KAAK,CAACe,MAAM,CAAE,oBAAmBD,OAAQ,EAAC,CAAC,CAACL,IAAI,CAAC,MAAM;QACrDF,IAAI,CAAC,CAAC;QACNS,MAAM,CAACC,KAAK,CAAC,SAAS,CAAC;MACzB,CAAC;IACH;IAEAV,IAAI,CAAC,CAAC;IAEN,OAAO;MAACF,KAAK;MAAEJ,GAAG;MAAEY;IAAM;EAC5B;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}