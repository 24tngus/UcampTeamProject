nenumber: \"\",\n        image: \"\"\n      }\n    });\n    const load = () => {\n      axios.get(\"/api/store/mypage\").then(({\n        data\n      }) => {\n        state.items = data;\n      });\n    };\n    load();\n    return {\n      state\n    };\n  }\n};","map":{"version":3,"names":["reactive","axios","router","Header2","name","components","data","seq","form","storename","detailinfo","category","location","phonenumber","image","created","currentRoute","value","params","load_shop","methods","get","then","deleteShop","delete","window","alert","push","path","myshopupdate","updateData","put","setup","state","items","Object","load"],"sources":["C:\\TeamProject\\VeganLoadMap\\src\\components\\shop\\MyshopUpdate.vue"],"sourcesContent":["<template>\r\n  <Header2 />\r\n  <div id=\"wrapper\">\r\n\r\n    <div id=\"container\">\r\n      <div class=\"tab\">\r\n        <router-link to=\"/mystore\"><h1>마이페이지</h1></router-link>\r\n        <div class=\"buttontab\">\r\n          <router-link to=\"/mystore_info\"><button class=\"tablink\">회원 정보</button></router-link>\r\n          <router-link to=\"/myshop_info\"><button class=\"tablink\">가게 정보</button></router-link>\r\n          <router-link to=\"/reserve_select\"><button class=\"tablink\">예약 확인</button></router-link>\r\n          <router-link to=\"/review_store\"><button class=\"tablink\">리뷰 확인</button></router-link>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 본문 작성 -->\r\n      <div class=\"online small\" id=\"online\">\r\n<!--        this.seq = router.currentRoute.value.params.value;-->\r\n<!--        <p>Received Value: {{$route.params.value}}</p>-->\r\n        <h1>{{state.items.name}}님 가게 정보 수정</h1>\r\n        <div class=\"member\">\r\n          <div class=\"field\">\r\n            <b>가게 이름</b>\r\n            <span class=\"placehold-text\">\r\n              <input type=\"text\" class=\"block\" v-model=\"this.form.storename\">\r\n      </span>\r\n          </div>\r\n          <div class=\"field\">\r\n            <b>