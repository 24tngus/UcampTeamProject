{"ast":null,"code":"import Header from \"@/components/header/Header.vue\";\nexport default {\n  components: {\n    Header\n  },\n  methods: {\n    async loadGoogleAPI() {\n      return new Promise(resolve => {\n        if (typeof gapi === 'undefined') {\n          const script = document.createElement('script');\n          script.src = 'https://apis.google.com/js/api.js';\n          script.onload = () => {\n            gapi.load('auth2', () => {\n              resolve();\n            });\n          };\n          document.head.appendChild(script);\n        } else {\n          resolve();\n        }\n      });\n    },\n    async onGoogleLogin() {\n      await this.loadGoogleAPI();\n      const googleAuth = gapi.auth2.init({\n        client_id: 'YOUR_CLIENT_ID' // 여기에 클라이언트 ID를 설정\n      });\n\n      googleAuth.signIn().then(this.onSignIn);\n    },\n    onSignIn(googleUser) {\n      const profile = googleUser.getBasicProfile();\n      console.log('ID: ' + profile.getId());\n      console.log('Name: ' + profile.getName());\n      console.log('Image URL: ' + profile.getImageUrl());\n      console.log('Email: ' + profile.getEmail());\n    },\n    async onGoogleLogout() {\n      await this.loadGoogleAPI();\n      const googleAuth = gapi.auth2.getAuthInstance();\n      googleAuth.signOut().then(() => {\n        console.log('User signed out.');\n      });\n    }\n  }\n};","map":{"version":3,"names":["Header","components","methods","loadGoogleAPI","Promise","resolve","gapi","script","document","createElement","src","onload","load","head","appendChild","onGoogleLogin","googleAuth","auth2","init","client_id","signIn","then","onSignIn","googleUser","profile","getBasicProfile","console","log","getId","getName","getImageUrl","getEmail","onGoogleLogout","getAuthInstance","signOut"],"sources":["C:\\VeganLoadMap\\src\\pages\\User.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <Header/>\r\n    <div class=\"form-signin w-100 m-auto\">\r\n      <button class=\"btn\" @click=\"$router.push('/login')\">사용자 로그인</button>\r\n      <button class=\"btn\" @click=\"$router.push('/login2')\">판매자 로그인</button>\r\n      <!-- 구글 로그인 버튼 -->\r\n      <div class=\"g-signin2 btn\" @click=\"onGoogleLogin\">구글 로그인</div>\r\n      <!-- 로그아웃 버튼 -->\r\n      <a href=\"#\" @click=\"onGoogleLogout\">Sign out</a>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Header from \"@/components/header/Header.vue\";\r\n\r\nexport default {\r\n  components: {Header},\r\n  methods: {\r\n    async loadGoogleAPI() {\r\n      return new Promise((resolve) => {\r\n        if (typeof gapi === 'undefined') {\r\n          const script = document.createElement('script');\r\n          script.src = 'https://apis.google.com/js/api.js';\r\n\r\n          script.onload = () => {\r\n            gapi.load('auth2', () => {\r\n              resolve();\r\n            });\r\n          };\r\n\r\n          document.head.appendChild(script);\r\n        } else {\r\n          resolve();\r\n        }\r\n      });\r\n    },\r\n    async onGoogleLogin() {\r\n      await this.loadGoogleAPI();\r\n      const googleAuth = gapi.auth2.init({\r\n        client_id: 'YOUR_CLIENT_ID', // 여기에 클라이언트 ID를 설정\r\n      });\r\n      googleAuth.signIn().then(this.onSignIn);\r\n    },\r\n    onSignIn(googleUser) {\r\n      const profile = googleUser.getBasicProfile();\r\n      console.log('ID: ' + profile.getId());\r\n      console.log('Name: ' + profile.getName());\r\n      console.log('Image URL: ' + profile.getImageUrl());\r\n      console.log('Email: ' + profile.getEmail());\r\n    },\r\n    async onGoogleLogout() {\r\n      await this.loadGoogleAPI();\r\n      const googleAuth = gapi.auth2.getAuthInstance();\r\n      googleAuth.signOut().then(() => {\r\n        console.log('User signed out.');\r\n      });\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.form-signin {\r\n  max-width: 330px;\r\n  padding: 1rem;\r\n}\r\n\r\n.btn {\r\n  background-color: #24855b;\r\n  color: #ffffff;\r\n}\r\n