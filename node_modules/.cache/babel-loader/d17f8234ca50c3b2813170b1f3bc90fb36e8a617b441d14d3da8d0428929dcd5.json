 progress-bar-striped progress-bar-animated\"\n                   role=\"progressbar\" :style=\"{ width: progressPercentage }\"></div>\n            </div>\n            <fieldset v-for=\"(step, index) in steps\" :key=\"index\">\n              <div class=\"form-card\" v-show=\"index === currentStep\">\n                <div class=\"row\">\n                  <div class=\"col-7\">\n                    <h2 class=\"fs-title\">{{ step.name }} Information:</h2>\n                  </div>\n                  <div class=\"col-5\">\n                    <h2 class=\"steps\">Step {{ index + 1 }} - 4</h2>\n                  </div>\n                </div>\n                <label class=\"fieldlabels\">Email: *</label>\n                <input type=\"email\" name=\"email\" placeholder=\"Email Id\" v-model=\"formData.email\" />\n                <label class=\"fieldlabels\">Username: *</label>\n                <input type=\"text\" name=\"uname\" placeholder=\"UserName\" v-model=\"formData.username\" />\n                <label class=\"fieldlabels\">Password: *</label>\n                <input type=\"password\" name=\"pwd\" placeholder=\"Password\" v-model=\"formData.password\" />\n                <label class=\"fieldlabels\">Confirm Password: *</label>\n                <input type=\"password\" name=\"cpwd\" placeholder=\"Confirm Password\" v-model=\"formData.confirmPassword\" />\n              </div>\n              <div class=\"form-card\" v-show=\"index === currentStep + 1\">\n                <div class=\"row\">\n                  <div class=\"col-7\">\n                    <h2 class=\"fs-title\">Personal Information:</h2>\n                  </div>\n                  <div class=\"col-5\">\n                    <h2 class=\"steps\">Step {{ index + 1 }} - 4</h2>\n                  </div>\n                </div>\n                <label class=\"fieldlabels\">First Name: *</label>\n                <input type=\"text\" name=\"fname\" placeholder=\"First Name\" v-model=\"formData.firstName\" />\n                <label class=\"fieldlabels\">Last Name: *</label>\n                <input type=\"text\" name=\"lname\" placeholder=\"Last Name\" v-model=\"formData.lastName\" />\n                <label class=\"fieldlabels\">Contact No.: *</label>\n                <input type=\"text\" name=\"phno\" placeholder=\"Contact No.\" v-model=\"formData.contactNo\" />\n                <label class=\"fieldlabels\">Alternate Contact No.: *</label>\n                <input type=\"text\" name=\"phno_2\" placeholder=\"Alternate Contact No.\" v-model=\"formData.alternateContactNo\" />\n              </div>\n              <div class=\"form-card\" v-show=\"index === currentStep + 2\">\n                <div class=\"row\">\n                  <div class=\"col-7\">\n                    <h2 class=\"fs-title\">Image Upload:</h2>\n                  </div>\n                  <div class=\"col-5\">\n                    <h2 class=\"steps\">Step {{ index + 1 }} - 4</h2>\n                  </div>\n                </div>\n                <label class=\"fieldlabels\">Upload Your Photo:</label>\n                <input type=\"file\" name=\"pic\" accept=\"image/*\" v-model=\"formData.photo\" />\n                <label class=\"fieldlabels\">Upload Signature Photo:</label>\n                <input type=\"file\" name=\"pic\" accept=\"image/*\" v-model=\"formData.signature\" />\n              </div>\n            </fieldset>\n            <button @click=\"prevStep\" v-if=\"currentStep > 0\">Previous</button>\n            <button @click=\"nextStep\" v-if=\"currentStep < steps.length - 1\">Next</button>\n          </form>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      currentStep: 0,\n      steps: [\n        { name: \"Account\" },\n        { name: \"Personal\" },\n        { name: \"Image\" },\n        { name: \"Finish\" }\n      ],\n      formData: {\n        email: '',\n        username: '',\n        password: '',\n        confirmPassword: '',\n        firstName: '',\n        lastName: '',\n        contactNo: '',\n        alternateContactNo: '',\n        photo: '',\n        signature: ''\n      }\n    };\n  },\n  computed: {\n    progressPercentage() {\n      return (this.currentStep / (this.steps.length - 1)) * 100 + '%';\n    }\n  },\n  methods: {\n    nextStep() {\n      if (this.currentStep < this.steps.length - 1) {\n        this.currentStep++;\n      }\n    },\n    prevStep() {\n      if (this.currentStep > 0) {\n        this.currentStep--;\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* CSS 스타일 내용을 이곳에 추가합니다. */\n</style>\n"],"mappings":"AAgFA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC;MACdC,KAAK,EAAE,CACL;QAAEC,IAAI,EAAE;MAAU,CAAC,EACnB;QAAEA,IAAI,EAAE;MAAW,CAAC,EACpB;QAAEA,IAAI,EAAE;MAAQ,CAAC,EACjB;QAAEA,IAAI,EAAE;MAAS,EAClB;MACDC,QAAQ,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,eAAe,EAAE,EAAE;QACnBC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,SAAS,EAAE,EAAE;QACbC,kBAAkB,EAAE,EAAE;QACtBC,KAAK,EAAE,EAAE;QACTC,SAAS,EAAE;MACb;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,kBAAkBA,CAAA,EAAG;MACnB,OAAQ,IAAI,CAACf,WAAU,IAAK,IAAI,CAACC,KAAK,CAACe,MAAK,GAAI,CAAC,CAAC,GAAI,GAAE,GAAI,GAAG;IACjE;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI,CAAClB,WAAU,GAAI,IAAI,CAACC,KAAK,CAACe,MAAK,GAAI,CAAC,EAAE;QAC5C,IAAI,CAAChB,WAAW,EAAE;MACpB;IACF,CAAC;IACDmB,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI,CAACnB,WAAU,GAAI,CAAC,EAAE;QACxB,IAAI,CAACA,WAAW,EAAE;MACpB;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}