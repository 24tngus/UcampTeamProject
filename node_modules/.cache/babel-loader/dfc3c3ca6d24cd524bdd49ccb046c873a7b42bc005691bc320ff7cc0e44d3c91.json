{"ast":null,"code":"import axios from \"axios\";\nimport { reactive } from \"vue\";\nimport lib from \"@/scripts/lib\";\nexport default {\n  name: \"Store\",\n  setup() {\n    const state = reactive({\n      menus: []\n    });\n    axios.get(\"/api/menu\").then(({\n      data\n    }) => {\n      state.menus = data;\n    });\n    return {\n      state,\n      lib\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","lib","name","setup","state","menus","get","then","data"],"sources":["C:\\JavaWork\\springboot\\VeganLoadMap\\src\\components\\Menu.vue"],"sourcesContent":["<template>\r\n  <div class=\"store\">\r\n    <div class=\"album py-5 bg-body-tertiary\">\r\n      <div class=\"container\">\r\n\r\n        <div class=\"row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3\">\r\n          <div class=\"col\" v-for=\"(menu, idx) in state.menus\" :key=\"idx\">\r\n            <div class=\"card shadow-sm\">\r\n              <!--      <span class=\"img\" style=\"{backgroundImage: url('${item.imagePath}')}\" />-->\r\n              <img :src=\"menu.image\"/>\r\n              <div class=\"card-body\">\r\n                <p class=\"card-text\">\r\n                  <span> {{menu.menu}} </span> &nbsp;\r\n                  <span class=\"discount badge bg-danger\">{{menu.sale}}%</span>\r\n                </p>\r\n                <div class=\"d-flex justify-content-between align-items-center\">\r\n                  <small class=\"price text-muted\">\r\n                    {{lib.getNumberFormatted(menu.price)}}원\r\n                  </small>\r\n                  <small class=\"real text-danger\">\r\n                    {{lib.getNumberFormatted(menu.price - (menu.price * menu.sale / 100)) }}원\r\n                  </small>\r\n                  <button class=\"btn btn-primary\" @click=\"$router.push('/reserve')\">\r\n                    <i class=\"fa fa-calendar-check-o\" aria-hidden=\"true\"></i>\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport {reactive} from \"vue\";\r\nimport lib from \"@/scripts/lib\";\r\n\r\nexport default {\r\n  name: \"Store\",\r\n  setup() {\r\n    const state = reactive({\r\n      menus: []\r\n    })\r\n    axios.get(\"/api/menu\").then(({data}) => {\r\n      state.menus = data;\r\n    })\r\n\r\n    return {state, lib};\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.card .img{\r\n  display: inline-block;\r\n  width: 100%;\r\n  hieght: 250px;\r\n  background-size: cover;\r\n  background-position: center;\r\n}\r\n\r\n.btn {\r\n  width:80px;\r\n  float: right;\r\n}\r\n\r\n.card .card-body .price{\r\n  text-decoration: line-through;\r\n}\r\n</style>"],"mappings":"AAsCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAAQC,QAAQ,QAAO,KAAK;AAC5B,OAAOC,GAAE,MAAO,eAAe;AAE/B,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIJ,QAAQ,CAAC;MACrBK,KAAK,EAAE;IACT,CAAC;IACDN,KAAK,CAACO,GAAG,CAAC,WAAW,CAAC,CAACC,IAAI,CAAC,CAAC;MAACC;IAAI,CAAC,KAAK;MACtCJ,KAAK,CAACC,KAAI,GAAIG,IAAI;IACpB,CAAC;IAED,OAAO;MAACJ,KAAK;MAAEH;IAAG,CAAC;EACrB;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}