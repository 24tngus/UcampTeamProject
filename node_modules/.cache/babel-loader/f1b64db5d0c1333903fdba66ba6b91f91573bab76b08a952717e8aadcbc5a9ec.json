{"ast":null,"code":"import axios from \"axios\";\nimport { reactive } from \"vue\";\nimport Header from \"@/components/header/Header.vue\";\n// import { mapState } from \"vuex\";\nimport VModal from 'vue-js-modal';\nexport default {\n  name: \"Shop\",\n  components: {\n    Header\n  },\n  data() {\n    return {\n      isModalVisible: false\n    };\n  },\n  methods: {\n    showModal() {\n      this.isModalVisible = true;\n    },\n    closeModal() {\n      this.isModalVisible = false;\n    },\n    ...mapState([\"selectedObject\"])\n  },\n  setup() {\n    const state = reactive({\n      items: []\n    });\n    axios.get(\"/api/shop\").then(({\n      data\n    }) => {\n      state.items = data;\n    });\n    const addToCart = shopSeq => {\n      axios.post(`/api/cart/shop/${shopSeq}`).then(() => {\n        console.log('success');\n      });\n    };\n    return {\n      state,\n      addToCart\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","Header","VModal","name","components","data","isModalVisible","methods","showModal","closeModal","mapState","setup","state","items","get","then","addToCart","shopSeq","post","console","log"],"sources":["/Users/soohykim/Desktop/front/src/pages/Shop.vue"],"sourcesContent":["<template>\n  <Header/>\n  <div class=\"shop\">\n    <div class=\"album py-5 bg-body-tertiary\">\n      <div class=\"container\">\n\n        <div class=\"row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3\">\n          <div class=\"col\" v-for=\"(shop, idx) in state.items\" :key=\"idx\">\n<!--            <h2>Detail Page</h2>-->\n<!--            <button @click=\"openModal\">View Details</button>-->\n<!--            <Modal :show=\"showModal\" @close=\"closeModal\">-->\n<!--              <h3>{{ selectedObject.storename }}</h3>-->\n<!--              <p>{{ selectedObject.description }}</p>-->\n<!--              &lt;!&ndash; 여기에 선택한 객체의 다른 상세 정보 표시 &ndash;&gt;-->\n<!--            </Modal>-->\n            <button @click=\"showModal\">Open Modal</button>\n            <modal :name=\"'my-modal'\" v-model=\"isModalVisible\">\n              <!-- 모달 내용을 여기에 넣습니다 -->\n              <h2>Modal Content</h2>\n              <p>This is the content of the modal.</p>\n            </modal>\n            <div class=\"card shadow-sm\">\n<!--              <div :style=\"`background-image : url(${shop.image})`\"></div>-->\n              <img :src=\"shop.image\"/>\n              <div class=\"card-body\">\n                <p class=\"card-text\">\n                  <span> {{shop.storename}} </span> &nbsp;\n                </p>\n                <div class=\"d-flex justify-content-between align-items-center\">\n                  <button class=\"btn btn-primary\" @click=\"$router.push('/menu')\">\n                    <i class=\"fa fa-plus\" aria-hidden=\"true\"></i>\n                  </button>\n                  <button class=\"btn btn-primary\" @click=\"addToCart(shop.seq)\">\n                    <i class=\"fa fa-shopping-cart\" aria-hidden=\"true\"></i>\n                  </button>\n                  <button class=\"btn btn-primary\" @click=\"$router.push('/reserve')\">\n                    <i class=\"fa fa-calendar-check-o\" aria-hidden=\"true\"></i>\n                  </button>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n  </div>\n\n</template>\n\n<script>\nimport axios from \"axios\";\nimport {reactive} from \"vue\";\nimport Header from \"@/components/header/Header.vue\";\n// import { mapState } from \"vuex\";\nimport VModal from 'vue-js-modal';\n\nexport default {\n  name: \"Shop\",\n  components: {Header},\n  data() {\n    return {\n      isModalVisible: false\n    };\n  },\n  methods: {\n    showModal() {\n      this.isModalVisible = true;\n    },\n    closeModal() {\n      this.isModalVisible = false;\n    },\n    ...mapState([\"selectedObject\"])\n  },\n  setup() {\n    const state = reactive({\n      items: []\n    })\n    axios.get(\"/api/shop\").then(({data}) => {\n      state.items = data;\n    })\n\n    const addToCart = (shopSeq) => {\n      axios.post(`/api/cart/shop/${shopSeq}`).then(() => {\n        console.log('success')\n      })\n    };\n\n    return {state, addToCart};\n  }\n}\n</script>\n\n<style scoped>\n.card .img{\n  display: inline-block;\n  width: 100%;\n  hieght: 250px;\n  background-size: cover;\n  background-position: center;\n}\n\n.btn {\n  width:80px;\n  float: right;\n}\n\n.card .card-body .price{\n  text-decoration: line-through;\n}\n</style>"],"mappings":"AAmDA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAAQC,QAAQ,QAAO,KAAK;AAC5B,OAAOC,MAAK,MAAO,gCAAgC;AACnD;AACA,OAAOC,MAAK,MAAO,cAAc;AAEjC,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IAACH;EAAM,CAAC;EACpBI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,IAAI,CAACF,cAAa,GAAI,IAAI;IAC5B,CAAC;IACDG,UAAUA,CAAA,EAAG;MACX,IAAI,CAACH,cAAa,GAAI,KAAK;IAC7B,CAAC;IACD,GAAGI,QAAQ,CAAC,CAAC,gBAAgB,CAAC;EAChC,CAAC;EACDC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIZ,QAAQ,CAAC;MACrBa,KAAK,EAAE;IACT,CAAC;IACDd,KAAK,CAACe,GAAG,CAAC,WAAW,CAAC,CAACC,IAAI,CAAC,CAAC;MAACV;IAAI,CAAC,KAAK;MACtCO,KAAK,CAACC,KAAI,GAAIR,IAAI;IACpB,CAAC;IAED,MAAMW,SAAQ,GAAKC,OAAO,IAAK;MAC7BlB,KAAK,CAACmB,IAAI,CAAE,kBAAiBD,OAAQ,EAAC,CAAC,CAACF,IAAI,CAAC,MAAM;QACjDI,OAAO,CAACC,GAAG,CAAC,SAAS;MACvB,CAAC;IACH,CAAC;IAED,OAAO;MAACR,KAAK;MAAEI;IAAS,CAAC;EAC3B;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}