{"ast":null,"code":"import Header from \"@/components/header/Header.vue\";\nexport default {\n  components: {\n    Header\n  },\n  methods: {\n    onGoogleLogin() {\n      // 이 부분에 Google 로그인 처리 로직을 넣어주세요.\n      gapi.load('auth2', function () {\n        gapi.auth2.init({\n          client_id: 'YOUR_CLIENT_ID' // 여기에 클라이언트 ID를 설정\n        }).then(() => {\n          const googleAuth = gapi.auth2.getAuthInstance();\n          googleAuth.signIn().then(this.onSignIn);\n        });\n      });\n    },\n    onSignIn(googleUser) {\n      const profile = googleUser.getBasicProfile();\n      console.log('ID: ' + profile.getId());\n      console.log('Name: ' + profile.getName());\n      console.log('Image URL: ' + profile.getImageUrl());\n      console.log('Email: ' + profile.getEmail());\n    },\n    onGoogleLogout() {\n      const googleAuth = gapi.auth2.getAuthInstance();\n      googleAuth.signOut().then(() => {\n        console.log('User signed out.');\n      });\n    }\n  }\n};","map":{"version":3,"names":["Header","components","methods","onGoogleLogin","gapi","load","auth2","init","client_id","then","googleAuth","getAuthInstance","signIn","onSignIn","googleUser","profile","getBasicProfile","console","log","getId","getName","getImageUrl","getEmail","onGoogleLogout","signOut"],"sources":["C:\\VeganLoadMap\\src\\pages\\User.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <Header/>\r\n    <div class=\"form-signin w-100 m-auto\">\r\n      <button class=\"btn\" @click=\"$router.push('/login')\">사용자 로그인</button>\r\n      <button class=\"btn\" @click=\"$router.push('/login2')\">판매자 로그인</button>\r\n      <!-- 구글 로그인 버튼 -->\r\n      <div class=\"g-signin2 btn\" @click=\"onGoogleLogin\">구글 로그인</div>\r\n      <!-- 로그아웃 버튼 -->\r\n      <a href=\"#\" @click=\"onGoogleLogout\">Sign out</a>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Header from \"@/components/header/Header.vue\";\r\n\r\nexport default {\r\n  components: {Header},\r\n  methods: {\r\n    onGoogleLogin() {\r\n      // 이 부분에 Google 로그인 처리 로직을 넣어주세요.\r\n      gapi.load('auth2', function () {\r\n        gapi.auth2.init({\r\n          client_id: 'YOUR_CLIENT_ID', // 여기에 클라이언트 ID를 설정\r\n        }).then(() => {\r\n          const googleAuth = gapi.auth2.getAuthInstance();\r\n          googleAuth.signIn().then(this.onSignIn);\r\n        });\r\n      });\r\n    },\r\n    onSignIn(googleUser) {\r\n      const profile = googleUser.getBasicProfile();\r\n      console.log('ID: ' + profile.getId());\r\n      console.log('Name: ' + profile.getName());\r\n      console.log('Image URL: ' + profile.getImageUrl());\r\n      console.log('Email: ' + profile.getEmail());\r\n    },\r\n    onGoogleLogout() {\r\n      const googleAuth = gapi.auth2.getAuthInstance();\r\n      googleAuth.signOut().then(() => {\r\n        console.log('User signed out.');\r\n      });\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.form-signin {\r\n  max-width: 330px;\r\n  padding: 1rem;\r\n}\r\n\r\n.btn {\r\n  background-color: #24855b;\r\n  color: #ffffff;\r\n}\r\n\r\n.form-signin .form-floating:focus-within {\r\n  z-index: 2;\r\n}\r\n</style>\r\n"],"mappings":"AAeA,OAAOA,MAAK,MAAO,gCAAgC;AAEnD,eAAe;EACbC,UAAU,EAAE;IAACD;EAAM,CAAC;EACpBE,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd;MACAC,IAAI,CAACC,IAAI,CAAC,OAAO,EAAE,YAAY;QAC7BD,IAAI,CAACE,KAAK,CAACC,IAAI,CAAC;UACdC,SAAS,EAAE,gBAAgB,CAAE;QAC/B,CAAC,CAAC,CAACC,IAAI,CAAC,MAAM;UACZ,MAAMC,UAAS,GAAIN,IAAI,CAACE,KAAK,CAACK,eAAe,CAAC,CAAC;UAC/CD,UAAU,CAACE,MAAM,CAAC,CAAC,CAACH,IAAI,CAAC,IAAI,CAACI,QAAQ,CAAC;QACzC,CAAC,CAAC;MACJ,CAAC,CAAC;IACJ,CAAC;IACDA,QAAQA,CAACC,UAAU,EAAE;MACnB,MAAMC,OAAM,GAAID,UAAU,CAACE,eAAe,CAAC,CAAC;MAC5CC,OAAO,CAACC,GAAG,CAAC,MAAK,GAAIH,OAAO,CAACI,KAAK,CAAC,CAAC,CAAC;MACrCF,OAAO,CAACC,GAAG,CAAC,QAAO,GAAIH,OAAO,CAACK,OAAO,CAAC,CAAC,CAAC;MACzCH,OAAO,CAACC,GAAG,CAAC,aAAY,GAAIH,OAAO,CAACM,WAAW,CAAC,CAAC,CAAC;MAClDJ,OAAO,CAACC,GAAG,CAAC,SAAQ,GAAIH,OAAO,CAACO,QAAQ,CAAC,CAAC,CAAC;IAC7C,CAAC;IACDC,cAAcA,CAAA,EAAG;MACf,MAAMb,UAAS,