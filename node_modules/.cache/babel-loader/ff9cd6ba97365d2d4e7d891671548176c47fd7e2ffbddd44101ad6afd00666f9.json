sonData = parseJsonData.results;\r\n          console.log(this.jsonData);\r\n          console.log(this.jsonData[1].name);\r\n        });\r\n  },\r\n  methods: {\r\n    showPlaceInfo: function () {\r\n      // let detailPlaceInfo;\r\n      fetch('https://proxy.cors.sh/https://maps.googleapis.com/maps/api/place/details/json?placeid=ChIJP1CJmGGcfDUR7ffziy-8NZA&key=AIzaSyB21eMRg-uMRk-i7r27DPDkntXbR5_kvnk', {\r\n        headers: {\r\n          'x-cors-api-key': 'temp_924d232f48d59dabc25af9778ab72f25'\r\n        }\r\n      })\r\n          .then(res => res.json())\r\n          .then(res => {\r\n            this.detailPlaceInfo = res.result;\r\n            console.log(this.detailPlaceInfo.name);\r\n            console.log(this.detailPlaceInfo.place_id);\r\n            console.log(this.detailPlaceInfo.geometry.location.lat);\r\n            console.log(this.detailPlaceInfo.geometry.location.lng);\r\n          })\r\n    },  // showPlaceInfo\r\n    setLocation: function () {\r\n\r\n    }\r\n\r\n  },\r\n})\r\n</script>"],"mappings":"AAmCA,SAAQA,eAAe,QAAO,KAAI;AAClC,SAAQC,SAAS,EAAEC,MAAM,EAAEC,aAAa,EAAEC,UAAU,QAAO,iBAAgB;AAE3E,eAAeJ,eAAe,CAAC;EAC7BK,UAAU,EAAE;IAACJ,SAAS;IAAEC,MAAM;IAAEC,aAAa;IAAEC;EAAU,CAAC;EAC1DE,KAAKA,CAAA,EAAG;IAGN,MAAMC,MAAK,GAAI,IAAI,CAACA,MAAM;IAE1B,MAAMC,SAAQ,GAAI,CAChB;MAACC,GAAG,EAAE,UAAU;MAAEC,GAAG,EAAE;IAAU,CAAC,EAClC;MAACD,GAAG,EAAE,QAAQ;MAAEC,GAAG,EAAE;IAAW,CAAC,CACnC;IAEA,OAAO;MAACH,MAAM;MAAEC;IAAS;EAC3B,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLJ,MAAM,EAAE,IAAI;MACZK,QAAO,EAAI,IAAI;MACfC,eAAc,EAAI;IACpB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAIC,aAAa;;IAEjB;IACAC,SAAS,CAACC,WAAW,CAACC,kBAAkB,CACnCC,QAAQ,IAAK;MACZ,MAAMC,YAAW,GAAI;QACnBX,GAAG,EAAEU,QAAQ,CAACE,MAAM,CAACC,QAAQ;QAC7BZ,GAAG,EAAES,QAAQ,CAACE,MAAM,CAACE;MACvB,CAAC;MACDC,OAAO,CAACC,GAAG,CAAC,iBAAgB,GAAIL,YAAY,CAACX,GAAG;MAChDe,OAAO,CAACC,GAAG,CAAC,iBAAgB,GAAIL,YAAY,CAACV,GAAG;MAChD,IAAI,CAACH,MAAK,GAAIa,YAAY;MAC1BI,OAAO,CAACC,GAAG,CAAC,aAAY,GAAI,IAAI,CAAClB,MAAM;IACzC,CAAC,EACAmB,KAAK,