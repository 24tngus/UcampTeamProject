{"ast":null,"code":"import Header from \"@/components/header/Header.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"ReserveTest\",\n  components: {\n    Header\n  },\n  data() {\n    return {\n      newdate: null,\n      reserves: []\n    };\n  },\n  created() {\n    this.fetchReserve();\n  },\n  methods: {\n    isFutureDate(date) {\n      const today = new Date();\n      const newDate = new Date(date);\n      return newDate > today;\n    },\n    fetchReserve() {\n      axios.get(\"/api/myreserve\").then(response => {\n        this.reserves = response.data.map(reserve => {\n          const timestamp = reserve.date;\n          const formattedDate = new Date(timestamp).toISOString().split('T')[0];\n          return {\n            ...reserve,\n            newdate: formattedDate\n          };\n        });\n        console.log(\"뉴데이트\" + this.reserves[0].newdate); // 첫 번째 예약의 newdate 확인\n      }).catch(error => {\n        console.error(\"리뷰 업데이트 중 오류 발생\", error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["Header","axios","name","components","data","newdate","reserves","created","fetchReserve","methods","isFutureDate","date","today","Date","newDate","get","then","response","map","reserve","timestamp","formattedDate","toISOString","split","console","log","catch","error"],"sources":["C:\\Users\\seokhyeon\\Desktop\\project111\\VeganLoadMap\\src\\components\\reserve\\ReserveTest.vue"],"sourcesContent":["<template>\r\n  <Header />\r\n  <div id=\"form-main\">\r\n    <div id=\"form-div\">\r\n      <div v-for=\"reserve in reserves\" :key=\"reserve.seq\">\r\n      <form class=\"form\" id=\"form1\">\r\n        식당 이름 : {{ reserve.storename }}\r\n        <p class=\"text\">\r\n          예약자 : <input name=\"name\" type=\"text\" v-model=\"reserve.reserver\" class=\"validate[required,custom[onlyLetter],length[0,100]] feedback-input\" id=\"name\" />\r\n        </p>\r\n        <p class=\"text\">\r\n          예약 인원 : <input name=\"name\" type=\"text\" v-model=\"reserve.people\" class=\"validate[required,custom[onlyLetter],length[0,100]] feedback-input\" id=\"name\" />\r\n        </p>\r\n        <p class=\"text\">\r\n          예약 날짜 : <input name=\"name\" type=\"text\" :value=\"reserve.newdate\" class=\"validate[required,custom[onlyLetter],length[0,100]] feedback-input\" id=\"name\" />\r\n        </p>\r\n        <p v-if=\"reserve.time0810 === 1\" class=\"text\">\r\n          예약 시간 : <input name=\"name\" type=\"text\" value=\"08:00 - 10:00\" class=\"validate[required,custom[onlyLetter],length[0,100]] feedback-input\" id=\"name\" />\r\n        </p>\r\n        <p v-else-if=\"reserve.time1012 === 1\" class=\"text\">\r\n          예약 시간 : <input name=\"name\" type=\"text\" value=\"10:00 - 12:00\" class=\"validate[required,custom[onlyLetter],length[0,100]] feedback-input\" id=\"name\" />\r\n        </p>\r\n        <p v-else-if=\"reserve.time1214 === 1\" class=\"text\">\r\n          예약 시간 : <input name=\"name\" type=\"text\" value=\"12:00 - 14:00\" class=\"validate[required,custom[onlyLetter],length[0,100]] feedback-input\" id=\"name\" />\r\n        </p>\r\n        <p v-else-if=\"reserve.time1416 === 1\" class=\"text\">\r\n          예약 시간 : <input name=\"name\" type=\"text\" value=\"14:00 - 16:00\" class=\"validate[required,custom[onlyLetter],length[0,100]] feedback-input\" id=\"name\" />\r\n        </p>\r\n        <p v-else-if=\"reserve.time1618 === 1\" class=\"text\">\r\n          예약 시간 : <input name=\"name\" type=\"text\" value=\"16:00 - 18:00\" class=\"validate[required,custom[onlyLetter],length[0,100]] feedback-input\" id=\"name\" />\r\n        </p>\r\n        <p v-else-if=\"reserve.time1820 === 1\" class=\"text\">\r\n          예약 시간 : <input name=\"name\" type=\"text\" value=\"18:00 - 20:00\" class=\"validate[required,custom[onlyLetter],length[0,100]] feedback-input\" id=\"name\" />\r\n        </p>\r\n        <p v-else-if=\"reserve.time2022 === 1\" class=\"text\">\r\n          예약 시간 : <input name=\"name\" type=\"text\" value=\"20:00 - 22:00\" class=\"validate[required,custom[onlyLetter],length[0,100]] feedback-input\" id=\"name\" />\r\n        </p>\r\n        <p class=\"text\">\r\n          추가 내용 : <input name=\"name\" type=\"text\" v-model=\"reserve.comment\" class=\"validate[required,custom[onlyLetter],length[0,100]] feedback-input\" id=\"name\" />\r\n        </p>\r\n\r\n\r\n        <div v-if=\"isFutureDate(reserve.newdate)\" class=\"submit\">\r\n          <input type=\"submit\" value=\"리뷰 쓰기\" id=\"button-blue\"/>\r\n          <div class=\"ease\"></div>\r\n        </div>\r\n      </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Header from \"@/components/header/Header.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"ReserveTest\",\r\n  components: {Header},\r\n  data(){\r\n    return {\r\n      newdate: null,\r\n      reserves: [],\r\n    };\r\n  },\r\n  created(){\r\n    this.fetchReserve();\r\n  },\r\n  methods:{\r\n    isFutureDate(date) {\r\n      const today = new Date();\r\n      const newDate = new Date(date);\r\n      return newDate > today;\r\n    },\r\n    fetchReserve() {\r\n      axios\r\n          .get(\"/api/myreserve\")\r\n          .then((response) => {\r\n            this.reserves = response.data.map((reserve) => {\r\n              const timestamp = reserve.date;\r\n              const formattedDate = new Date(timestamp).toISOString().split('T')[0];\r\n              return {\r\n                ...reserve,\r\n                newdate: formattedDate,\r\n              };\r\n            });\r\n            console.log(\"뉴데이트\" + this.reserves[0].newdate); // 첫 번째 예약의 newdate 확인\r\n          })\r\n          .catch((error) => {\r\n            console.error(\"리뷰 업데이트 중 오류 발생\", error);\r\n          });\r\n    }\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import url(https://fonts.googleapis.com/css?family=Montserrat:400,700);\r\n\r\nhtml {\r\n  background: url(http://thekitemap.com/images/f